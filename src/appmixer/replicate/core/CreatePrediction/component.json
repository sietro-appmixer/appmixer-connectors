{
    "name": "appmixer.replicate.core.CreatePrediction",
    "description": "Creates a new machine learning prediction using either an official model (owner/model-name format) or a specific model version (with version hash).",
    "author": "Appmixer <info@appmixer.com>",
    "version": "1.0.0",
    "auth": {
        "service": "appmixer:replicate"
    },
    "quota": {
        "manager": "appmixer:replicate",
        "resources": "requests"
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "version": {
                        "type": "string"
                    },
                    "input": {
                        "type": "string"
                    },
                    "webhook": {
                        "type": "string"
                    },
                    "webhookEventsFilter": {
                        "type": "array"
                    }
                },
                "required": [
                    "version",
                    "input"
                ]
            },
            "inspector": {
                "inputs": {
                    "version": {
                        "type": "text",
                        "label": "Model Version or Official Model",
                        "index": 1,
                        "tooltip": "For official models, use format 'owner/model-name' (e.g., 'meta/meta-llama-3-70b-instruct'). For custom models, use the full version ID (e.g., 'owner/model:hash' or just the 64-character hash)."
                    },
                    "input": {
                        "type": "textarea",
                        "label": "Input Object",
                        "index": 2,
                        "tooltip": "The input object for the model. Refer to the model's API documentation for the required format. Examples: {\"prompt\": \"Hello world\"} for text models, {\"img\": \"image_url\"} for image models, or any other parameters required by the specific model."
                    },
                    "webhook": {
                        "type": "text",
                        "label": "Webhook URL",
                        "index": 3,
                        "tooltip": "Optional webhook URL to receive notifications when the prediction status changes."
                    },
                    "webhookEventsFilter": {
                        "type": "multiselect",
                        "label": "Webhook Events Filter",
                        "index": 4,
                        "tooltip": "Select the events to filter.",
                        "options": [
                            {
                                "label": "Start",
                                "value": "start"
                            },
                            {
                                "label": "Output",
                                "value": "output"
                            },
                            {
                                "label": "Completed",
                                "value": "completed"
                            },
                            {
                                "label": "Logs",
                                "value": "logs"
                            }
                        ]
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "options": [
                {
                    "label": "Prediction Id",
                    "value": "id",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Model",
                    "value": "model",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Version",
                    "value": "version",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Input",
                    "value": "input",
                    "schema": {
                        "type": "object",
                        "properties": {
                            "text": {
                                "type": "string",
                                "title": "Input.Text"
                            }
                        }
                    }
                },
                {
                    "label": "Logs",
                    "value": "logs",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Output",
                    "value": "output",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Data Removed",
                    "value": "data_removed",
                    "schema": {
                        "type": "boolean"
                    }
                },
                {
                    "label": "Error",
                    "value": "error",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Status",
                    "value": "status",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Created At",
                    "value": "created_at",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Urls",
                    "value": "urls",
                    "schema": {
                        "type": "object",
                        "properties": {
                            "cancel": {
                                "type": "string",
                                "title": "Urls.Cancel"
                            },
                            "get": {
                                "type": "string",
                                "title": "Urls.Get"
                            },
                            "web": {
                                "type": "string",
                                "title": "Urls.Web"
                            }
                        }
                    }
                }
            ]
        }
    ],
    "icon": "data:image/svg+xml;base64,"
}