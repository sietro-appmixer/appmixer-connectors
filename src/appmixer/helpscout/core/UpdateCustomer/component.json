{
    "name": "appmixer.helpscout.core.UpdateCustomer",
    "author": "Appmixer <info@appmixer.com>",
    "description": "Update an existing customer via validated JSON Patch operations (common top-level fields exposed).",
    "version": "1.0.0",
    "private": false,
    "auth": {
        "service": "appmixer:helpscout"
    },
    "quota": {
        "manager": "appmixer:helpscout",
        "resources": "requests"
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "integer"
                    },
                    "firstName": {
                        "type": "string"
                    },
                    "lastName": {
                        "type": "string"
                    },
                    "background": {
                        "type": "string"
                    },
                    "addEmailValue": {
                        "type": "string"
                    },
                    "addEmailType": {
                        "type": "string"
                    },
                    "removeEmailId": {
                        "type": "integer"
                    },
                    "replaceEmailId": {
                        "type": "integer"
                    },
                    "replaceEmailValue": {
                        "type": "string"
                    },
                    "replaceEmailType": {
                        "type": "string"
                    }
                },
                "required": [
                    "id"
                ]
            },
            "inspector": {
                "inputs": {
                    "id": {
                        "type": "number",
                        "tooltip": "Customer ID.",
                        "label": "Customer ID",
                        "index": 0
                    },
                    "firstName": {
                        "type": "text",
                        "tooltip": "First name.",
                        "label": "First Name",
                        "index": 1
                    },
                    "lastName": {
                        "type": "text",
                        "tooltip": "Last name.",
                        "label": "Last Name",
                        "index": 2
                    },
                    "background": {
                        "type": "text",
                        "tooltip": "Internal notes/background (replace).",
                        "label": "Background",
                        "index": 3
                    },
                    "addEmailValue": {
                        "type": "text",
                        "tooltip": "Add email - address value.",
                        "label": "Add Email - Value",
                        "index": 4
                    },
                    "addEmailType": {
                        "type": "text",
                        "tooltip": "Add email - type (work, home, other).",
                        "label": "Add Email - Type",
                        "index": 5
                    },
                    "removeEmailId": {
                        "type": "number",
                        "tooltip": "Remove the email with this emailId (use emails list endpoint to get ids).",
                        "label": "Remove Email ID",
                        "index": 6,
                        "source": {
                            "url": "/component/appmixer/helpscout/core/ListEmails?outPort=out",
                            "data": {
                                "transform": "./ListEmails#emailsToSelectArray",
                                "properties": {
                                    "isSource": true
                                },
                                "messages": {
                                    "in/id": "inputs/in/id"
                                }
                            }
                        }
                    },
                    "replaceEmailId": {
                        "type": "text",
                        "tooltip": "Replace email value/type for this emailId.",
                        "label": "Replace Email ID",
                        "index": 7,
                        "source": {
                            "url": "/component/appmixer/helpscout/core/ListEmails?outPort=out",
                            "data": {
                                "transform": "./ListEmails#emailsToSelectArray",
                                "properties": {
                                    "isSource": true
                                },
                                "messages": {
                                    "in/id": "inputs/in/id"
                                }
                            }
                        }
                    },
                    "replaceEmailValue": {
                        "type": "text",
                        "tooltip": "New email value for replace operation.",
                        "label": "Replace Email - Value",
                        "index": 8
                    },
                    "replaceEmailType": {
                        "type": "text",
                        "tooltip": "New email type for replace operation.",
                        "label": "Replace Email - Type",
                        "index": 9
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "options": []
        }
    ],
    "icon": "data:image/svg+xml;base64,"
}