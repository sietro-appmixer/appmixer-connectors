{
    "name": "appmixer.helpscout.core.CreateCustomer",
    "author": "Appmixer <info@appmixer.com>",
    "description": "Create a new customer profile in HelpScout with comprehensive customer information.",
    "version": "1.0.0",
    "private": false,
    "auth": {
        "service": "appmixer:helpscout"
    },
    "quota": {
        "manager": "appmixer:helpscout",
        "resources": "requests"
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "firstName": {
                        "type": "string"
                    },
                    "lastName": {
                        "type": "string"
                    },
                    "emailValue": {
                        "type": "string"
                    },
                    "emailType": {
                        "type": "string"
                    },
                    "phone": {
                        "type": "string"
                    },
                    "photoUrl": {
                        "type": "string"
                    },
                    "photoType": {
                        "type": "string"
                    },
                    "jobTitle": {
                        "type": "string"
                    },
                    "location": {
                        "type": "string"
                    },
                    "background": {
                        "type": "string"
                    },
                    "age": {
                        "type": "string"
                    },
                    "gender": {
                        "type": "string"
                    }
                },
                "required": ["firstName", "lastName", "emailValue"]
            },
            "inspector": {
                "inputs": {
                    "firstName": {
                        "type": "text",
                        "tooltip": "First name of the customer.",
                        "label": "First Name",
                        "index": 1
                    },
                    "lastName": {
                        "type": "text",
                        "tooltip": "Last name of the customer.",
                        "label": "Last Name",
                        "index": 2
                    },
                    "emailValue": {
                        "type": "text",
                        "tooltip": "Email address of the customer.",
                        "label": "Email Address",
                        "index": 3
                    },
                    "emailType": {
                        "type": "select",
                        "tooltip": "Type of email address.",
                        "label": "Email Type",
                        "index": 4,
                        "defaultValue": "work",
                        "options": [
                            { "clearItem": true, "content": "-- Clear selection --" },
                            {
                                "label": "Work",
                                "value": "work"
                            },
                            {
                                "label": "Home",
                                "value": "home"
                            },
                            {
                                "label": "Other",
                                "value": "other"
                            }
                        ]
                    },
                    "phone": {
                        "type": "text",
                        "tooltip": "Phone number of the customer. Format: 222-333-4444",
                        "label": "Phone Number",
                        "index": 5
                    },
                    "photoUrl": {
                        "type": "text",
                        "tooltip": "URL of the customer's photo (max 200 characters).",
                        "label": "Photo URL",
                        "index": 6
                    },
                    "photoType": {
                        "type": "select",
                        "tooltip": "Type of photo.",
                        "label": "Photo Type",
                        "index": 7,
                        "defaultValue": "unknown",
                        "options": [
                            { "clearItem": true, "content": "-- Clear selection --" },
                            {
                                "label": "Unknown",
                                "value": "unknown"
                            },
                            {
                                "label": "Gravatar",
                                "value": "gravatar"
                            },
                            {
                                "label": "Twitter",
                                "value": "twitter"
                            },
                            {
                                "label": "Facebook",
                                "value": "facebook"
                            },
                            {
                                "label": "Google Profile",
                                "value": "googleprofile"
                            },
                            {
                                "label": "Google Plus",
                                "value": "googleplus"
                            },
                            {
                                "label": "LinkedIn",
                                "value": "linkedin"
                            },
                            {
                                "label": "Instagram",
                                "value": "instagram"
                            }
                        ]
                    },
                    "jobTitle": {
                        "type": "text",
                        "tooltip": "Job title of the customer.",
                        "label": "Job Title",
                        "index": 8
                    },
                    "location": {
                        "type": "text",
                        "tooltip": "Location of the customer.",
                        "label": "Location",
                        "index": 9
                    },
                    "background": {
                        "type": "textarea",
                        "tooltip": "Background notes about the customer (max 200 characters).",
                        "label": "Background Notes",
                        "index": 10
                    },
                    "age": {
                        "type": "text",
                        "tooltip": "Age or age range of the customer.",
                        "label": "Age",
                        "index": 11
                    },
                    "gender": {
                        "type": "select",
                        "tooltip": "Gender of the customer.",
                        "label": "Gender",
                        "index": 12,
                        "defaultValue": "unknown",
                        "options": [
                            { "clearItem": true, "content": "-- Clear selection --" },
                            {
                                "label": "Unknown",
                                "value": "unknown"
                            },
                            {
                                "label": "Male",
                                "value": "male"
                            },
                            {
                                "label": "Female",
                                "value": "female"
                            }
                        ]
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "options": [
                {
                    "label": "Customer ID",
                    "value": "Resource-ID",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Customer Location URL",
                    "value": "Location",
                    "schema": {
                        "type": "string"
                    }
                }
            ]
        }
    ],
    "icon": "data:image/svg+xml;base64,"
}