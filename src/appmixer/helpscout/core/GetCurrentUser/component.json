                {
                    "label": "Role",
                    "value": "role",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Timezone",
                    "value": "timezone",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Photo Url",
                    "value": "photoUrl",
                    "schema": {
                        "type": "string"
                    }
