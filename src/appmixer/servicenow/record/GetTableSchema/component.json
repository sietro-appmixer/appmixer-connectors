{
    "name": "appmixer.servicenow.record.GetTableSchema",
    "author": "Appmixer <info@appmixer.ai>",
    "description": "Retrieves the record identified by the specified sys_id from the specified table.",
    "version": "1.0.0",
    "private": false,
    "auth": {
        "service": "appmixer:servicenow"
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "properties": {
                    "tableName": {
                        "type": "string"
                    }
                },
                "required": [
                    "tableName"
                ]
            },
            "inspector": {
                "inputs": {
                    "tableName": {
                        "type": "text",
                        "label": "Table Name",
                        "index": 1,
                        "tooltip": "Name of the table to retrieve the schema for."
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out"
        }
    ],
    "icon": "data:image/svg+xml;base64,"
}
