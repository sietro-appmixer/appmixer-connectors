{
    "name": "appmixer.kit.sequence.AddSubscriberToSequence",
    "label": "Add Subscriber to Sequence",
    "author": "Appmixer <info@appmixer.com>",
    "description": "Adds a subscriber to a specified sequence.",
    "version": "1.0.0",
    "private": false,
    "auth": {
        "service": "appmixer:kit"
    },
    "quota": {
        "manager": "appmixer:kit",
        "resources": "requests"
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "sequenceId": {
                        "type": "string"
                    },
                    "subscriberId": {
                        "type": "string"
                    }
                },
                "required": ["sequenceId", "subscriberId"]
            },
            "inspector": {
                "inputs": {
                    "sequenceId": {
                        "type": "text",
                        "tooltip": "Unique identifier of the sequence.",
                        "label": "Sequence ID",
                        "index": 0,
                        "source": {
                            "url": "/component/appmixer/kit/sequence/ListSequences?outPort=out",
                            "data": {
                                "message": {
                                    "in/outputType": "array"
                                },
                                "properties": {
                                    "isSource": true
                                },
                                "transform": "./ListSequences#toSelectArray"
                            }
                        }
                    },
                    "subscriberId": {
                        "type": "text",
                        "tooltip": "Unique identifier of the subscriber.",
                        "label": "Subscriber ID",
                        "index": 1,
                        "source": {
                            "url": "/component/appmixer/kit/subscriber/FindSubscribers?outPort=out",
                            "data": {
                                "message": {
                                    "in/outputType": "array"
                                },
                                "properties": {
                                    "isSource": true
                                },
                                "transform": "./FindSubscribers#toSelectArray"
                            }
                        }
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "options": [
                {
                    "label": "Subscriber ID",
                    "value": "id",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "First Name",
                    "value": "first_name",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Email Address",
                    "value": "email_address",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "State",
                    "value": "state",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Created At",
                    "value": "created_at",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Added At",
                    "value": "added_at",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Fields",
                    "value": "fields",
                    "schema": {
                        "type": "object",
                        "properties": {}
                    }
                }
            ]
        }
    ],
    "icon": "data:image/png;base64,"
}
