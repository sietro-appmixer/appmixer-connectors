{
    "name": "appmixer.kit.subscriber.ListTagsForSubscriber",
    "label": "List Tags for Subscriber",
    "author": "Appmixer <info@appmixer.com>",
    "description": "Retrieve a list of available tags for a specific subscriber.",
    "version": "1.0.0",
    "private": false,
    "auth": {
        "service": "appmixer:kit"
    },
    "quota": {
        "manager": "appmixer:kit",
        "resources": "requests"
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "subscriberId": {
                        "type": "string"
                    },
                    "outputType": {
                        "type": "string"
                    }
                },
                "required": ["subscriberId"]
            },
            "inspector": {
                "inputs": {
                    "subscriberId": {
                        "type": "text",
                        "tooltip": "Unique identifier of the subscriber.",
                        "label": "Subscriber ID",
                        "index": 0,
                        "source": {
                            "url": "/component/appmixer/kit/subscriber/FindSubscribers?outPort=out",
                            "data": {
                                "message": {
                                    "in/outputType": "array"
                                },
                                "properties": {
                                    "isSource": true
                                },
                                "transform": "./FindSubscribers#toSelectArray"
                            }
                        }
                    },
                    "outputType": {
                        "type": "select",
                        "label": "Output Type",
                        "index": 1,
                        "defaultValue": "array",
                        "tooltip": "Choose whether you want to receive the result set as one complete list, or first item only or one item at a time or stream the items to a file.",
                        "options": [
                            {
                                "label": "First Item Only",
                                "value": "first"
                            },
                            {
                                "label": "All items at once",
                                "value": "array"
                            },
                            {
                                "label": "One item at a time",
                                "value": "object"
                            },
                            {
                                "label": "Store to CSV file",
                                "value": "file"
                            }
                        ]
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "source": {
                "url": "/component/appmixer/kit/subscriber/ListTagsForSubscriber?outPort=out",
                "data": {
                    "properties": {
                        "generateOutputPortOptions": true
                    },
                    "messages": {
                        "in/subscriberId": "123",
                        "in/outputType": "inputs/in/outputType"
                    }
                }
            }
        }
    ],
    "icon": "data:image/png;base64,"
}
