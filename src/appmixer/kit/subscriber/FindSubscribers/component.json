{
    "name": "appmixer.kit.subscriber.FindSubscribers",
    "label": "Find Subscribers",
    "author": "Appmixer <info@appmixer.com>",
    "description": "Retrieve a list of subscribers.",
    "version": "1.0.0",
    "private": false,
    "auth": {
        "service": "appmixer:kit"
    },
    "quota": {
        "manager": "appmixer:kit",
        "resources": "requests"
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "status": {
                        "type": "string"
                    },
                    "emailAddress": {
                        "type": "string"
                    },
                    "createdAfter": {
                        "type": "string"
                    },
                    "createdBefore": {
                        "type": "string"
                    },
                    "updatedAfter": {
                        "type": "string"
                    },
                    "updatedBefore": {
                        "type": "string"
                    },
                    "outputType": {
                        "type": "string"
                    }
                }
            },
            "inspector": {
                "inputs": {
                    "emailAddress": {
                        "type": "text",
                        "tooltip": "Filter subscribers by email address.",
                        "label": "Email Address",
                        "index": 0
                    },
                    "status": {
                        "type": "select",
                        "tooltip": "Filter subscribers by status.",
                        "label": "Status",
                        "index": 1,
                        "defaultValue": "active",
                        "options": [
                            {
                                "label": "Active (Default)",
                                "value": "active"
                            },
                            {
                                "label": "Inactive",
                                "value": "inactive"
                            },
                            {
                                "label": "Bounced",
                                "value": "bounced"
                            },
                            {
                                "label": "Complained",
                                "value": "complained"
                            },
                            {
                                "label": "Cancelled",
                                "value": "cancelled"
                            },
                            {
                                "label": "All",
                                "value": "all"
                            }
                        ]
                    },
                    "createdAfter": {
                        "type": "date-time",
                        "tooltip": "Filter subscribers created after this date.",
                        "label": "Created After",
                        "index": 2,
                        "config": {
                            "enableTime": false,
                            "format": "YYYY-MM-DD"
                        }
                    },
                    "createdBefore": {
                        "type": "date-time",
                        "tooltip": "Filter subscribers created before this date.",
                        "label": "Created Before",
                        "index": 3,
                        "config": {
                            "enableTime": false,
                            "format": "YYYY-MM-DD"
                        }
                    },
                    "updatedAfter": {
                        "type": "date-time",
                        "tooltip": "Filter subscribers updated after this date.",
                        "label": "Updated After",
                        "index": 4,
                        "config": {
                            "enableTime": false,
                            "format": "YYYY-MM-DD"
                        }
                    },
                    "updatedBefore": {
                        "type": "date-time",
                        "tooltip": "Filter subscribers updated before this date.",
                        "label": "Updated Before",
                        "index": 5,
                        "config": {
                            "enableTime": false,
                            "format": "YYYY-MM-DD"
                        }
                    },
                    "outputType": {
                        "type": "select",
                        "label": "Output Type",
                        "index": 6,
                        "defaultValue": "array",
                        "tooltip": "Choose whether you want to receive the result set as one complete list, or first item only or one item at a time or stream the items to a file.",
                        "options": [
                            {
                                "label": "First Item Only",
                                "value": "first"
                            },
                            {
                                "label": "All items at once",
                                "value": "array"
                            },
                            {
                                "label": "One item at a time",
                                "value": "object"
                            },
                            {
                                "label": "Store to CSV file",
                                "value": "file"
                            }
                        ]
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "source": {
                "url": "/component/appmixer/kit/subscriber/FindSubscribers?outPort=out",
                "data": {
                    "properties": {
                        "generateOutputPortOptions": true
                    },
                    "messages": {
                        "in/outputType": "inputs/in/outputType"
                    }
                }
            }
        },
        {
            "name": "notFound"
        }
    ],
    "icon": "data:image/png;base64,"
}
