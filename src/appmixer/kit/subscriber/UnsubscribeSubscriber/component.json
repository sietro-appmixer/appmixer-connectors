{
    "name": "appmixer.kit.subscriber.UnsubscribeSubscriber",
    "label": "Unsubscribe Subscriber",
    "author": "Appmixer <info@appmixer.com>",
    "description": "Mark a subscriber as unsubscribed using their unique identifier.",
    "version": "1.0.0",
    "private": false,
    "auth": {
        "service": "appmixer:kit"
    },
    "quota": {
        "manager": "appmixer:kit",
        "resources": "requests"
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "subscriberId": {
                        "type": "string"
                    }
                },
                "required": ["subscriberId"]
            },
            "inspector": {
                "inputs": {
                    "subscriberId": {
                        "type": "text",
                        "tooltip": "Unique identifier of the subscriber.",
                        "label": "Subscriber ID",
                        "index": 0,
                        "source": {
                            "url": "/component/appmixer/kit/subscriber/FindSubscribers?outPort=out",
                            "data": {
                                "message": {
                                    "in/outputType": "array"
                                },
                                "properties": {
                                    "isSource": true
                                },
                                "transform": "./FindSubscribers#toSelectArray"
                            }
                        }
                    }
                }
            }
        }
    ],
    "outPorts": ["out"],
    "icon": "data:image/png;base64,"
}
