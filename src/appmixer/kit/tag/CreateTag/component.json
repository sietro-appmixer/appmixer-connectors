{
    "name": "appmixer.kit.tag.CreateTag",
    "label": "Create Tag",
    "description": "Create a new tag in Kit.",
    "author": "Appmixer <info@appmixer.com>",
    "version": "1.0.0",
    "auth": {
        "service": "appmixer:kit"
    },
    "quota": {
        "manager": "appmixer:kit",
        "resources": "requests"
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "name": {
                        "type": "string"
                    }
                },
                "required": ["name"]
            },
            "inspector": {
                "inputs": {
                    "name": {
                        "label": "Tag Name",
                        "type": "text",
                        "index": 0,
                        "tooltip": "The name of the tag to create. Must be unique within your Kit account."
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "options": [
                { "label": "Tag ID", "value": "id", "schema": {"type": "string"} },
                { "label": "Name", "value": "name", "schema": {"type": "string"} },
                { "label": "Created At", "value": "created_at", "schema": {"type": "string"} }
            ]
        }
    ],
    "icon": "data:image/png;base64,"
}
