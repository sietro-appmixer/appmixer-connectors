{
    "name": "appmixer.github.project.FindProjects",
    "label": "Find Projects",
    "description": "Retrieve all projects accessible to the authenticated user (either via organization or personal projects) using GitHub's GraphQL API. Supports filtering by project name using the query parameter.",
    "author": "Appmixer <info@appmixer.com>",
    "version": "1.0.0",
    "auth": {
        "service": "appmixer:github",
        "scope": [
            "read:project"
        ]
    },
    "quota": {
        "manager": "appmixer:github",
        "resources": "requests-projects",
        "scope": {
            "userId": "{{userId}}"
        }
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "owner": {
                        "type": "string",
                        "description": "The username or organization name whose projects are to be retrieved"
                    },
                    "projectType": {
                        "type": "string",
                        "enum": ["user", "organization"],
                        "description": "Type of project to look for"
                    },
                    "query": {
                        "type": "string",
                        "description": "Optional search query to filter projects by name (case-insensitive partial match)"
                    },
                    "outputType": {
                        "type": "string",
                        "enum": ["array", "first", "object", "file"],
                        "description": "Output format"
                    }
                },
                "required": ["owner"]
            },
            "inspector": {
                "inputs": {
                    "owner": {
                        "type": "text",
                        "tooltip": "The username or organization name whose projects are to be retrieved.",
                        "label": "Owner",
                        "index": 1
                    },
                    "projectType": {
                        "type": "select",
                        "tooltip": "Type of project to look for",
                        "label": "Project Type",
                        "index": 2,
                        "defaultValue": "user",
                        "options": [
                            {
                                "label": "User Projects",
                                "value": "user"
                            },
                            {
                                "label": "Organization Projects",
                                "value": "organization"
                            }
                        ]
                    },
                    "query": {
                        "type": "text",
                        "tooltip": "Optional search query to filter projects by name (case-insensitive partial match). Leave empty to get all projects.",
                        "label": "Query",
                        "index": 3
                    },
                    "outputType": {
                        "type": "select",
                        "label": "Output Type",
                        "index": 4,
                        "defaultValue": "array",
                        "tooltip": "Choose whether you want to receive the result set as one complete list, or first item only or one item at a time or stream the items to a file.",
                        "options": [
                            {
                                "label": "First Item Only",
                                "value": "first"
                            },
                            {
                                "label": "All items at once",
                                "value": "array"
                            },
                            {
                                "label": "One item at a time",
                                "value": "object"
                            },
                            {
                                "label": "Store to CSV file",
                                "value": "file"
                            }
                        ]
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "source": {
                "url": "/component/appmixer/github/project/FindProjects?outPort=out",
                "data": {
                    "properties": {
                        "generateOutputPortOptions": true
                    },
                    "messages": {
                        "in/outputType": "inputs/in/outputType",
                        "in/owner": "any"
                    }
                }
            }
        },
        {
            "name": "notFound"
        }
    ],
    "icon": "data:image/svg+xml;base64,"
}