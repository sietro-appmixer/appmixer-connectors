{
    "name": "appmixer.github.project.GetProject",
    "label": "Get Project",
    "description": "Retrieve detailed information for a specific GitHub project.",
    "author": "Appmixer <info@appmixer.com>",
    "version": "1.0.0",
    "auth": {
        "service": "appmixer:github",
        "scope": [
            "read:project"
        ]
    },
    "quota": {
        "manager": "appmixer:github",
        "resources": "requests-projects",
        "scope": {
            "userId": "{{userId}}"
        }
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "projectId": {
                        "type": "string",
                        "description": "The unique identifier of the project"
                    }
                },
                "required": ["projectId"]
            },
            "inspector": {
                "inputs": {
                    "projectId": {
                        "type": "text",
                        "tooltip": "The unique identifier of the project.",
                        "label": "Project ID",
                        "index": 1
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "options": [
                {
                    "label": "ID",
                    "value": "id",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Number",
                    "value": "number",
                    "schema": {
                        "type": "number"
                    }
                },
                {
                    "label": "Title",
                    "value": "title",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Short Description",
                    "value": "shortDescription",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Readme",
                    "value": "readme",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Url",
                    "value": "url",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Created At",
                    "value": "createdAt",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Updated At",
                    "value": "updatedAt",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Closed At",
                    "value": "closedAt",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Public",
                    "value": "public",
                    "schema": {
                        "type": "boolean"
                    }
                },
                {
                    "label": "Owner",
                    "value": "owner",
                    "schema": {
                        "type": "object",
                        "properties": {
                            "__typename": {
                                "type": "string",
                                "title": "Owner.Typename"
                            },
                            "login": {
                                "type": "string",
                                "title": "Owner.Login"
                            }
                        }
                    }
                },
                {
                    "label": "Fields",
                    "value": "fields",
                    "schema": {
                        "type": "array",
                        "items": {
                            "type": "object",
                            "properties": {
                                "id": {
                                    "type": "string",
                                    "title": "Fields.Id"
                                },
                                "name": {
                                    "type": "string",
                                    "title": "Fields.Name"
                                },
                                "dataType": {
                                    "type": "string",
                                    "title": "Fields.Data Type"
                                },
                                "options": {
                                    "type": "array",
                                    "items": {
                                        "type": "object"
                                    },
                                    "title": "Fields.Options"
                                }
                            }
                        }
                    }
                }
            ]
        }
    ],
    "icon": "data:image/svg+xml;base64,"
}