{
    "name": "appmixer.clerk.core.DeleteEmail",
    "author": "Appmixer <info@appmixer.com>",
    "description": "Delete an email address from a user's account.",
    "version": "1.0.0",
    "private": false,
    "auth": {
        "service": "appmixer:clerk"
    },
    "quota": {
        "manager": "appmixer:clerk",
        "resources": "requests"
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "id": { "type": "string" }
                },
                "required": ["id"]
            },
            "inspector": {
                "inputs": {
                    "id": {
                        "type": "text",
                        "tooltip": "Unique identifier of the email to be deleted.",
                        "label": "Email ID",
                        "index": 0
                    }
                }
            }
        }
    ],
    "outPorts": ["out"],
    "icon": "data:image/svg+xml;base64,"
}
