{
    "name": "appmixer.harvest.project.CreateProject",
    "author": "Appmixer <info@appmixer.com>",
    "description": "Create a new project assigned to a client.",
    "version": "1.0.0",
    "private": false,
    "auth": {
        "service": "appmixer:harvest"
    },
    "quota": {
        "manager": "appmixer:harvest",
        "resources": "requests"
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "clientId": {
                        "type": "string"
                    },
                    "name": {
                        "type": "string"
                    },
                    "isBillable": {
                        "type": "boolean"
                    },
                    "billBy": {
                        "type": "string"
                    },
                    "budgetBy": {
                        "type": "string"
                    },
                    "code": {
                        "type": "string"
                    },
                    "isFixedFee": {
                        "type": "boolean"
                    },
                    "hourlyRate": {
                        "type": "number"
                    },
                    "budget": {
                        "type": "number"
                    },
                    "budgetIsMonthly": {
                        "type": "boolean"
                    },
                    "notes": {
                        "type": "string"
                    },
                    "isActive": {
                        "type": "boolean"
                    }
                },
                "required": [
                    "clientId",
                    "name",
                    "billBy",
                    "budgetBy"
                ]
            },
            "inspector": {
                "inputs": {
                    "clientId": {
                        "type": "text",
                        "tooltip": "Client ID to associate the project with.",
                        "label": "Client ID",
                        "index": 0
                    },
                    "name": {
                        "type": "text",
                        "label": "Project Name",
                        "index": 1
                    },
                    "isBillable": {
                        "type": "toggle",
                        "tooltip": "Whether the project is billable or not.",
                        "label": "Is Billable",
                        "index": 2,
                        "defaultValue": false
                    },
                    "billBy": {
                        "type": "select",
                        "tooltip": "The method by which the project is invoiced",
                        "label": "Bill By",
                        "index": 3,
                        "options": [
                            { "label": "None", "value": "none" },
                            { "label": "Project", "value": "Project" },
                            { "label": "People", "value": "People" },
                            { "label": "Tasks", "value": "Tasks" }
                        ],
                        "defaultValue": "none"
                    },
                    "budgetBy": {
                        "type": "select",
                        "tooltip": "The method by which the project is budgeted",
                        "label": "Budget By",
                        "index": 4,
                        "options": [
                            { "label": "No Budget", "value": "none" },
                            { "label": "Hours Per Project", "value": "project" },
                            { "label": "Total Project Fees", "value": "project_cost" },
                            { "label": "Hours Per Task", "value": "task" },
                            { "label": "Fees Per Task", "value": "task_fees" },
                            { "label": "Hours Per Person", "value": "person" }
                        ],
                        "defaultValue": "none"
                    },
                    "code": {
                        "type": "text",
                        "tooltip": "The code associated with the project",
                        "label": "Code",
                        "index": 5
                    },
                    "isFixedFee": {
                        "type": "toggle",
                        "tooltip": "Whether the project has a fixed-fee or not.",
                        "label": "Is Fixed Fee",
                        "index": 6,
                        "defaultValue": false
                    },
                    "hourlyRate": {
                        "type": "number",
                        "tooltip": "Default hourly rate if applicable.",
                        "label": "Hourly Rate",
                        "index": 7
                    },
                    "budget": {
                        "type": "number",
                        "tooltip": "Total budget for project.",
                        "label": "Budget",
                        "index": 8
                    },
                    "budgetIsMonthly": {
                        "type": "toggle",
                        "tooltip": "If true, budget resets monthly.",
                        "label": "Budget Is Monthly",
                        "index": 9,
                        "defaultValue": false
                    },
                    "notes": {
                        "type": "text",
                        "tooltip": "Project notes.",
                        "label": "Notes",
                        "index": 10
                    },
                    "isActive": {
                        "type": "toggle",
                        "tooltip": "Set active or archived.",
                        "label": "Is Active",
                        "index": 11,
                        "defaultValue": true
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "options": [
                {
                    "label": "Project ID",
                    "value": "id",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Project Name",
                    "value": "name",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Code",
                    "value": "code",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Is Active",
                    "value": "is_active",
                    "schema": {
                        "type": "boolean"
                    }
                },
                {
                    "label": "Is Billable",
                    "value": "is_billable",
                    "schema": {
                        "type": "boolean"
                    }
                },
                {
                    "label": "Is Fixed Fee",
                    "value": "is_fixed_fee",
                    "schema": {
                        "type": "boolean"
                    }
                },
                {
                    "label": "Bill By",
                    "value": "bill_by",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Budget",
                    "value": "budget",
                    "schema": {
                        "type": "integer"
                    }
                },
                {
                    "label": "Budget By",
                    "value": "budget_by",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Budget Is Monthly",
                    "value": "budget_is_monthly",
                    "schema": {
                        "type": "boolean"
                    }
                },
                {
                    "label": "Notify When Over Budget",
                    "value": "notify_when_over_budget",
                    "schema": {
                        "type": "boolean"
                    }
                },
                {
                    "label": "Over Budget Notification Percentage",
                    "value": "over_budget_notification_percentage",
                    "schema": {
                        "type": "integer"
                    }
                },
                {
                    "label": "Show Budget To All",
                    "value": "show_budget_to_all",
                    "schema": {
                        "type": "boolean"
                    }
                },
                {
                    "label": "Created At",
                    "value": "created_at",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Updated At",
                    "value": "updated_at",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Starts On",
                    "value": "starts_on",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Ends On",
                    "value": "ends_on",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Over Budget Notification Date",
                    "value": "over_budget_notification_date",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Notes",
                    "value": "notes",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Cost Budget",
                    "value": "cost_budget",
                    "schema": {
                        "type": "number"
                    }
                },
                {
                    "label": "Cost Budget Include Expenses",
                    "value": "cost_budget_include_expenses",
                    "schema": {
                        "type": "boolean"
                    }
                },
                {
                    "label": "Hourly Rate",
                    "value": "hourly_rate",
                    "schema": {
                        "type": "integer"
                    }
                },
                {
                    "label": "Fee",
                    "value": "fee",
                    "schema": {
                        "type": "number"
                    }
                },
                {
                    "label": "Client",
                    "value": "client",
                    "schema": {
                        "type": "object",
                        "properties": {
                            "id": {
                                "type": "integer",
                                "title": "Client.ID"
                            },
                            "name": {
                                "type": "string",
                                "title": "Client.Name"
                            },
                            "currency": {
                                "type": "string",
                                "title": "Client.Currency"
                            }
                        }
                    }
                }
            ]
        }
    ],
    "icon": "data:image/png;base64,"
}
