{
    "name": "appmixer.harvest.project.UpdateProject",
    "author": "Appmixer <info@appmixer.com>",
    "description": "Update an existing project.",
    "version": "1.0.0",
    "private": false,
    "auth": {
        "service": "appmixer:harvest"
    },
    "quota": {
        "manager": "appmixer:harvest",
        "resources": "requests"
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "projectId": {
                        "type": "string"
                    },
                    "clientId": {
                        "type": "string"
                    },
                    "name": {
                        "type": "string"
                    },
                    "isBillable": {
                        "type": "boolean"
                    },
                    "billBy": {
                        "type": "string"
                    },
                    "budgetBy": {
                        "type": "string"
                    },
                    "code": {
                        "type": "string"
                    },
                    "isFixedFee": {
                        "type": "boolean"
                    },
                    "hourlyRate": {
                        "type": "number"
                    },
                    "budget": {
                        "type": "number"
                    },
                    "budgetIsMonthly": {
                        "type": "boolean"
                    },
                    "notes": {
                        "type": "string"
                    },
                    "isActive": {
                        "type": "boolean"
                    }
                },
                "required": [
                    "projectId"
                ]
            },
            "inspector": {
                "inputs": {
                    "projectId": {
                        "type": "text",
                        "label": "Project ID",
                        "index": 0
                    },
                    "clientId": {
                        "type": "text",
                        "tooltip": "Client ID to associate the project with.",
                        "label": "Client ID",
                        "index": 1
                    },
                    "name": {
                        "type": "text",
                        "label": "Project Name",
                        "index": 2
                    },
                    "isBillable": {
                        "type": "toggle",
                        "tooltip": "Whether the project is billable or not.",
                        "label": "Is Billable",
                        "index": 3
                    },
                    "billBy": {
                        "type": "select",
                        "tooltip": "The method by which the project is invoiced",
                        "label": "Bill By",
                        "index": 4,
                        "options": [
                            { "label": "<Empty>", "clearItem": true },
                            { "label": "None", "value": "none" },
                            { "label": "Project", "value": "Project" },
                            { "label": "People", "value": "People" },
                            { "label": "Tasks", "value": "Tasks" }
                        ]
                    },
                    "budgetBy": {
                        "type": "select",
                        "tooltip": "The method by which the project is budgeted",
                        "label": "Budget By",
                        "index": 5,
                        "options": [
                            { "label": "<Empty>", "clearItem": true },
                            { "label": "No Budget", "value": "none" },
                            { "label": "Hours Per Project", "value": "project" },
                            { "label": "Total Project Fees", "value": "project_cost" },
                            { "label": "Hours Per Task", "value": "task" },
                            { "label": "Fees Per Task", "value": "task_fees" },
                            { "label": "Hours Per Person", "value": "person" }
                        ]
                    },
                    "code": {
                        "type": "text",
                        "tooltip": "The code associated with the project",
                        "label": "Code",
                        "index": 6
                    },
                    "isFixedFee": {
                        "type": "toggle",
                        "tooltip": "Whether the project has a fixed-fee or not.",
                        "label": "Is Fixed Fee",
                        "index": 7
                    },
                    "hourlyRate": {
                        "type": "number",
                        "tooltip": "Default hourly rate if applicable.",
                        "label": "Hourly Rate",
                        "index": 8
                    },
                    "budget": {
                        "type": "number",
                        "tooltip": "Total budget for project.",
                        "label": "Budget",
                        "index": 9
                    },
                    "budgetIsMonthly": {
                        "type": "toggle",
                        "tooltip": "If true, budget resets monthly.",
                        "label": "Budget Is Monthly",
                        "index": 10
                    },
                    "notes": {
                        "type": "text",
                        "tooltip": "Project notes.",
                        "label": "Notes",
                        "index": 11
                    },
                    "isActive": {
                        "type": "toggle",
                        "tooltip": "Set active or archived.",
                        "label": "Is Active",
                        "index": 12
                    }
                }
            }
        }
    ],
    "outPorts": [
        "out"
    ],
    "icon": "data:image/png;base64,"
}
