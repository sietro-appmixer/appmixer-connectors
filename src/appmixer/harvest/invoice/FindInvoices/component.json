{
    "name": "appmixer.harvest.invoice.FindInvoices",
    "author": "Appmixer <info@appmixer.com>",
    "description": "Find invoices in Harvest based on client, state, or date range. This component will return a maximum of 2000 records.",
    "version": "1.0.0",
    "private": false,
    "auth": {
        "service": "appmixer:harvest"
    },
    "quota": {
        "manager": "appmixer:harvest",
        "resources": "requests"
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "clientId": {
                        "type": "string"
                    },
                    "projectId": {
                        "type": "string"
                    },
                    "state": {
                        "type": "string"
                    },
                    "from": {
                        "type": "string"
                    },
                    "to": {
                        "type": "string"
                    },
                    "updatedSince": {
                        "type": "string"
                    },
                    "outputType": {
                        "type": "string"
                    }
                }
            },
            "inspector": {
                "inputs": {
                    "clientId": {
                        "type": "text",
                        "tooltip": "Filter by client ID.",
                        "label": "Client ID",
                        "index": 0
                    },
                    "projectId": {
                        "type": "text",
                        "tooltip": "Filter by project ID.",
                        "label": "Project ID",
                        "index": 1
                    },
                    "state": {
                        "type": "select",
                        "tooltip": "Filter by invoice state.",
                        "label": "State",
                        "index": 2,
                        "options": [
                            {
                                "label": "All States",
                                "clearItem": true
                            },
                            {
                                "label": "Draft",
                                "value": "draft"
                            },
                            {
                                "label": "Open",
                                "value": "open"
                            },
                            {
                                "label": "Paid",
                                "value": "paid"
                            },
                            {
                                "label": "Closed",
                                "value": "closed"
                            }
                        ]
                    },
                    "from": {
                        "type": "date-time",
                        "tooltip": "Only return invoices with an issue_date on or after this date. Format: YYYY-MM-DD.",
                        "label": "From Date",
                        "index": 3,
                        "config": {
                            "enableTime": false,
                            "format": "YYYY-MM-DD"
                        }
                    },
                    "to": {
                        "type": "date-time",
                        "tooltip": "Only return invoices with an issue_date on or before this date. Format: YYYY-MM-DD.",
                        "label": "To Date",
                        "index": 4,
                        "config": {
                            "enableTime": false,
                            "format": "YYYY-MM-DD"
                        }
                    },
                    "updatedSince": {
                        "type": "date-time",
                        "tooltip": "Only return invoices updated since this datetime (ISO 8601).",
                        "label": "Updated Since",
                        "index": 5
                    },
                    "outputType": {
                        "type": "select",
                        "label": "Output Type",
                        "index": 6,
                        "defaultValue": "array",
                        "tooltip": "Choose whether you want to receive the result set as one complete list, or first item only or one item at a time or stream the items to a file.",
                        "options": [
                            {
                                "label": "First Item Only",
                                "value": "first"
                            },
                            {
                                "label": "All items at once",
                                "value": "array"
                            },
                            {
                                "label": "One item at a time",
                                "value": "object"
                            },
                            {
                                "label": "Store to CSV file",
                                "value": "file"
                            }
                        ]
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "source": {
                "url": "/component/appmixer/harvest/invoice/FindInvoices?outPort=out",
                "data": {
                    "properties": {
                        "generateOutputPortOptions": true
                    },
                    "messages": {
                        "in/outputType": "inputs/in/outputType"
                    }
                }
            }
        },
        {
            "name": "notFound"
        }
    ],
    "icon": "data:image/png;base64,"
}
