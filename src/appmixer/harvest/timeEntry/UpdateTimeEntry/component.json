{
    "name": "appmixer.harvest.timeEntry.UpdateTimeEntry",
    "author": "Appmixer <info@appmixer.com>",
    "description": "Update an existing time entry. You can update entries via duration (hours) or by start and end time.",
    "version": "1.0.0",
    "private": false,
    "auth": {
        "service": "appmixer:harvest"
    },
    "quota": {
        "manager": "appmixer:harvest",
        "resources": "requests"
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "timeEntryId": {
                        "type": "string"
                    },
                    "type": {
                        "type": "string"
                    },
                    "projectId": {
                        "type": "string"
                    },
                    "taskId": {
                        "type": "string"
                    },
                    "userId": {
                        "type": "string"
                    },
                    "spentDate": {
                        "type": "string"
                    },
                    "hours": {
                        "type": "number"
                    },
                    "startedTime": {
                        "type": "string"
                    },
                    "endedTime": {
                        "type": "string"
                    },
                    "notes": {
                        "type": "string"
                    }
                },
                "required": [
                    "timeEntryId"
                ]
            },
            "inspector": {
                "inputs": {
                    "timeEntryId": {
                        "type": "text",
                        "label": "Time Entry ID",
                        "tooltip": "The unique identifier of the time entry to update.",
                        "index": 0
                    },
                    "type": {
                        "type": "select",
                        "label": "Timer mode",
                        "index": 1,
                        "tooltip": "Choose how to update the time entry: via duration (hours) or start/end time. You should use the method that you have configured in your Harvest account settings.",
                        "options": [
                            {
                                "label": "Track time via duration (Hours)",
                                "value": "duration"
                            },
                            {
                                "label": "Track time via start and end time",
                                "value": "start_end_time"
                            }
                        ],
                        "defaultValue": "duration"
                    },
                    "projectId": {
                        "type": "text",
                        "tooltip": "The ID of the project to associate with the time entry.",
                        "label": "Project ID",
                        "index": 2
                    },
                    "taskId": {
                        "type": "text",
                        "tooltip": "The ID of the task to associate with the time entry. The task needs to be also assigned to the project.",
                        "label": "Task ID",
                        "index": 3
                    },
                    "userId": {
                        "type": "text",
                        "tooltip": "The ID of the user to associate with the time entry. Defaults to the currently authenticated user.",
                        "label": "User ID",
                        "index": 4
                    },
                    "spentDate": {
                        "type": "date-time",
                        "tooltip": "The date the time entry was spent. Format: YYYY-MM-DD.",
                        "label": "Spent Date",
                        "index": 5,
                        "config": {
                            "enableTime": false,
                            "format": "YYYY-MM-DD"
                        }
                    },
                    "hours": {
                        "type": "number",
                        "tooltip": "The current amount of time tracked. If provided, the time entry will be updated with the specified hours and is_running will be set to false. If not provided, hours will be set to 0.0 and is_running will be set to true.",
                        "label": "Hours",
                        "index": 6,
                        "when": { "eq": { "./type": "duration" } }
                    },
                    "startedTime": {
                        "type": "text",
                        "tooltip": "The time the entry started (e.g., '8:00am').",
                        "label": "Started Time",
                        "index": 6,
                        "when": { "eq": { "./type": "start_end_time" } }
                    },
                    "endedTime": {
                        "type": "text",
                        "tooltip": "The time the entry ended (e.g., '5:00pm').",
                        "label": "Ended Time",
                        "index": 7,
                        "when": { "eq": { "./type": "start_end_time" } }
                    },
                    "notes": {
                        "type": "textarea",
                        "tooltip": "Any notes to be associated with the time entry.",
                        "label": "Notes",
                        "index": 8
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out"
        }
    ],
    "icon": "data:image/png;base64,"
}
