{
    "name": "appmixer.railway.core.CreateProject",
    "description": "Create a new project with a given name, description, and team ID.",
    "author": "Appmixer <info@appmixer.com>",
    "version": "1.0.0",
    "label": "Create Project",
    "auth": {
        "service": "appmixer:railway"
    },
    "quota": {
        "manager": "appmixer:railway",
        "resources": "requests"
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "name": {
                        "type": "string"
                    },
                    "description": {
                        "type": "string"
                    },
                    "teamId": {
                        "type": "string"
                    }
                },
                "required": [
                    "name"
                ]
            },
            "inspector": {
                "inputs": {
                    "name": {
                        "type": "text",
                        "tooltip": "Name of the new project.",
                        "label": "Name",
                        "index": 0
                    },
                    "description": {
                        "type": "text",
                        "tooltip": "Description for the project.",
                        "label": "Description",
                        "index": 1
                    },
                    "teamId": {
                        "type": "text",
                        "tooltip": "Team identifier to assign the project.",
                        "label": "Team Id",
                        "index": 2
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "options": [
                {
                    "label": "Project Id",
                    "value": "id",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Project Name",
                    "value": "name",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Description",
                    "value": "description",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Team Id",
                    "value": "teamId",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Created At",
                    "value": "createdAt",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Updated At",
                    "value": "updatedAt",
                    "schema": {
                        "type": "string"
                    }
                }
            ]
        }
    ],
    "icon": "data:image/svg+xml;base64,"
}