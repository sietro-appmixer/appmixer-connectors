{
    "name": "appmixer.railway.core.GetService",
    "description": "Retrieve detailed information about a specific service.",
    "author": "Appmixer <info@appmixer.com>",
    "version": "1.0.0",
    "label": "Get Service",
    "auth": {
        "service": "appmixer:railway"
    },
    "quota": {
        "manager": "appmixer:railway",
        "resources": "requests"
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "serviceId": {
                        "type": "string"
                    }
                },
                "required": [
                    "serviceId"
                ]
            },
            "inspector": {
                "inputs": {
                    "serviceId": {
                        "type": "text",
                        "tooltip": "Identifier of the service to retrieve.",
                        "label": "Service Id",
                        "index": 0
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "options": [
                {
                    "label": "Typename",
                    "value": "__typename",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Created At",
                    "value": "createdAt",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Deleted At",
                    "value": "deletedAt",
                    "schema": {
                        "type": "null"
                    }
                },
                {
                    "label": "Feature Flags",
                    "value": "featureFlags",
                    "schema": {
                        "type": "array",
                        "items": {}
                    }
                },
                {
                    "label": "Icon",
                    "value": "icon",
                    "schema": {
                        "type": "null"
                    }
                },
                {
                    "label": "Service Id",
                    "value": "id",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Service Name",
                    "value": "name",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Project Id",
                    "value": "projectId",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Template Service Id",
                    "value": "templateServiceId",
                    "schema": {
                        "type": "null"
                    }
                },
                {
                    "label": "Template Thread Slug",
                    "value": "templateThreadSlug",
                    "schema": {
                        "type": "null"
                    }
                },
                {
                    "label": "Updated At",
                    "value": "updatedAt",
                    "schema": {
                        "type": "string"
                    }
                }
            ]
        }
    ],
    "icon": "data:image/svg+xml;base64,"
}