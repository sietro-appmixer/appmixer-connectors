        {
            "title": "Find Projects",
            "description": "Retrieve all projects accessible to the authenticated user.",
            "type": "action",
            "documentation": "https://docs.railway.com/guides/manage-projects",
            "endpointUrl": "https://backboard.railway.com/graphql/v2",
            "apiBaseUrl": "https://backboard.railway.com",
            "method": "POST",
            "sampleResponse": {
                "data": {
                    "me": {
                        "projects": {
                            "edges": [
                                {
                                    "node": {
                                        "id": "proj_123",
                                        "name": "Example Project",
                                        "services": [],
                                        "environments": []
                                    }
                                }
                            ]
                        }
                    }
                }
            },
            "inputSchema": {
                "type": "object",
                "properties": {
                    "query": {
                        "type": "string",
                        "description": "Optional search string to filter projects."
                    }
                }
            },
            "outputType": {
                "outputType": "array",
                "arrayPropertyValue": "data.me.projects.edges"
            }
        },
        {
            "title": "Create Project",
