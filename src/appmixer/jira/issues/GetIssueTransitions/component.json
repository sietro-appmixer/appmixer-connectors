{
    "name": "appmixer.jira.issues.GetIssueTransitions",
    "description": "Get a selected Issue using its ID.",
    "author": "Appmixer <info@appmixer.com>",
    "label": "Find Issue Transitions",
    "private": false,
    "auth": {
        "service": "appmixer:jira",
        "scope": [
            "read:jira-work"
        ]
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "id": { "type": "string" },
                    "name": { "type": "string" },
                    "outputType": { "type": "string" }
                },
                "required": ["id"]
            },
            "inspector": {
                "inputs": {
                    "id": {
                        "type": "text",
                        "label": "Issue ID or Key",
                        "index": 1,
                        "tooltip": "Enter the ID or Key of the issue."
                    },
                    "name": {
                        "type": "text",
                        "label": "Name",
                        "index": 1,
                        "tooltip": "Enter the name of the transition you wish to look up. If you leave this field blank, it will display all available transitions."
                    },
                    "outputType": {
                        "type": "select",
                        "label": "Output Type",
                        "index": 2,
                        "defaultValue": "array",
                        "tooltip": "Choose whether you want to receive the result set as one complete list, or first item only or one item at a time or stream the items to a file.",
                        "options": [
                            {
                                "label": "First Item Only",
                                "value": "first"
                            },
                            {
                                "label": "All items at once",
                                "value": "array"
                            },
                            {
                                "label": "One item at a time",
                                "value": "object"
                            },
                            {
                                "label": "Store to CSV file",
                                "value": "file"
                            }
                        ]
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "source": {
                "url": "/component/appmixer/jira/issues/GetIssueTransitions?outPort=out",
                "data": {
                    "properties": {
                        "generateOutputPortOptions": true
                    },
                    "messages": {
                        "in/id": "any",
                        "in/outputType": "inputs/in/outputType"
                    }
                }
            }
        },
        {
            "name": "notFound",
            "options": [
                {
                    "label": "Issue ID",
                    "value": "id"
                },
                {
                    "label": "Name",
                    "value": "name"
                }
            ]
        }
    ],
    "icon": "data:image/svg+xml;base64,"
}
