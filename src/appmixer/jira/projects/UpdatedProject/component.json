{
    "name": "appmixer.jira.projects.UpdatedProject",
    "description": "Triggers when a project is updated (Webhook).",
    "author": "Jimoh Damilola <jimoh@client.io>",
    "webhook": true,
    "auth": {
        "service": "appmixer:jira",
        "scope": [
            "read:jira-work"
        ]
    },
    "properties": {
        "schema": {
            "properties": {
                "generateInspector": { "type": "boolean" },
                "url": {}
            }
        },
        "inspector": {
            "inputs": {
                "url": {
                    "source": {
                        "url": "/component/appmixer/jira/projects/UpdatedProject?outPort=project",
                        "data": { "properties": { "generateInspector": true } }
                    }
                }
            }
        }
    },
    "outPorts": [
        {
            "name": "project",
            "options": [
                { "label": "Project ID", "value": "id" },
                { "label": "Project Key", "value": "key" },
                { "label": "Project Name", "value": "name" },
                { "label": "Project Description", "value": "description" },
                { "label": "Project Lead", "value": "lead" },
                { "label": "Lead Key", "value": "lead.key" },
                { "label": "Lead Account ID", "value": "lead.accountId" },
                { "label": "Lead Name", "value": "lead.name" },
                { "label": "Lead Avatar URLs", "value": "lead.avatarUrls" },
                { "label": "Lead Display Name", "value": "lead.displayName" },
                { "label": "Lead Status", "value": "lead.active" },
                { "label": "Project Components", "value": "components" },
                { "label": "Project Issue Types", "value": "issueTypes" },
                { "label": "Project Email", "value": "email" },
                { "label": "Project URL", "value": "url" },
                { "label": "Project Assignee Type", "value": "assigneeType" },
                { "label": "Project versions", "value": "versions" },
                { "label": "Project Roles", "value": "roles" },
                { "label": "Project Avatar URLs", "value": "avatarUrls" },
                { "label": "Project Category", "value": "projectCategory" },
                { "label": "Category ID", "value": "projectCategory.id" },
                { "label": "Category Name", "value": "projectCategory.name" },
                { "label": "Category Description", "value": "projectCategory.description" },
                { "label": "Project Simplified", "value": "simplified" },
                { "label": "Project Style", "value": "style" },
                { "label": "Project Properties", "value": "properties" },
                { "label": "Project Insight", "value": "insight" }
            ]
        }
    ],
    "icon": "data:image/svg+xml;base64,"
}
