{
    "name": "appmixer.jira.projects.UpdateProject",
    "author": "Jimoh Damilola <jimoh@client.io>",
    "description": "Update a Jira project. It does not work on Cloud Free Plan accounts.",
    "private": false,
    "auth": {
        "service": "appmixer:jira",
        "scope": [
            "manage:jira-configuration"
        ]
    },
    "inPorts": [{
        "name": "in",
        "schema": {
            "type": "object",
            "properties": {
                "id": { "type": "string" },
                "key": { "type": "string" },
                "name": { "type": "string" },
                "assigneeType": { "type": "string" },
                "leadAccountId": { "type": "string" },
                "description": { "type": "string" },
                "categoryId": { "type": "string" },
                "url": { "type": "string" },
                "avatarId": { "type": "string" },
                "issueSecurityScheme": { "type": "string" },
                "permissionScheme": { "type": "string" },
                "notificationScheme": { "type": "string" }
            },
            "required": [
                "id"
            ]
        },
        "inspector": {
            "inputs": {
                "id": {
                    "type": "text",
                    "label": "Project ID",
                    "index": 0,
                    "tooltip": "The ID of the project",
                    "source": {
                        "url": "/component/appmixer/jira/projects/ListProjects?outPort=projects",
                        "data": {
                            "transform": "./ListProjects#projectsToSelectArray"
                        }
                    }
                },
                "key": {
                    "type": "text",
                    "label": "Project Key",
                    "index": 1,
                    "tooltip": "Project keys must be unique and start with an uppercase letter followed by one or more uppercase alphanumeric characters. The maximum length is 10 characters."
                },
                "name": {
                    "type": "text",
                    "label": "Project Name",
                    "index": 2,
                    "tooltip": "The name of the project."
                },
                "leadAccountId": {
                    "type": "text",
                    "label": "Lead Account ID",
                    "tooltip": "The account ID of the project lead.",
                    "index": 4,
                    "source": {
                        "url": "/component/appmixer/jira/issues/ListField?outPort=out",
                        "data": {
                            "messages": { "in": { "type": "assignee" } },
                            "transform": "./ListField#fieldToSelectArray"
                        }
                    }
                },
                "description": {
                    "type": "text",
                    "tooltip": "A brief description of the project.",
                    "label": "Description",
                    "index": 5
                },
                "categoryId": {
                    "type": "text",
                    "label": "Category ID",
                    "index": 6,
                    "tooltip": "The ID of the project's category."
                },
                "assigneeType": {
                    "type": "select",
                    "label": "Assignee Type",
                    "index": 7,
                    "tooltip": "The default assignee when creating issues for this project.",
                    "options": [
                        { "value": "PROJECT_LEAD", "content": "Project Lead" },
                        { "value": "UNASSIGNED", "content": "Unassigned" }
                    ],
                    "defaultValue": "UNASSIGNED"
                },
                "url": {
                    "type": "text",
                    "label": "Project URL",
                    "index": 8,
                    "tooltip": "A link to information about this project, such as project documentation."
                },
                "avatarId": {
                    "type": "text",
                    "label": "Avatar ID",
                    "index": 9,
                    "tooltip": "An integer value for the project's avatar."
                },
                "issueSecurityScheme": {
                    "type": "text",
                    "label": "Security Issue",
                    "index": 10,
                    "tooltip": "The ID of the issue security scheme for the project, which enables you to control who can and cannot view issues."
                },
                "permissionScheme": {
                    "type": "text",
                    "label": "Permission Scheme",
                    "index": 11,
                    "tooltip": "The ID of the permission scheme for the project."
                },
                "notificationScheme": {
                    "type": "text",
                    "label": "Notification Scheme",
                    "index": 12,
                    "tooltip": "The ID of the notification scheme for the project"
                }
            }
        }
    }],
    "outPorts": [
        {
            "name": "project",
            "options": [
                { "label": "Project ID", "value": "id" },
                { "label": "Project Key", "value": "key" },
                { "label": "Project Name", "value": "name" },
                { "label": "Project Description", "value": "description" },
                { "label": "Project Lead", "value": "lead" },
                { "label": "Lead Key", "value": "lead.key" },
                { "label": "Lead Account ID", "value": "lead.accountId" },
                { "label": "Lead Name", "value": "lead.name" },
                { "label": "Lead Avatar URLs", "value": "lead.avatarUrls" },
                { "label": "Lead Display Name", "value": "lead.displayName" },
                { "label": "Lead Status", "value": "lead.active" },
                { "label": "Project Components", "value": "components" },
                { "label": "Project Issue Types", "value": "issueTypes" },
                { "label": "Project Email", "value": "email" },
                { "label": "Project URL", "value": "url" },
                { "label": "Project Assignee Type", "value": "assigneeType" },
                { "label": "Project versions", "value": "versions" },
                { "label": "Project Roles", "value": "roles" },
                { "label": "Project Avatar URLs", "value": "avatarUrls" },
                { "label": "Project Category", "value": "projectCategory" },
                { "label": "Category ID", "value": "projectCategory.id" },
                { "label": "Category Name", "value": "projectCategory.name" },
                { "label": "Category Description", "value": "projectCategory.description" },
                { "label": "Project Simplified", "value": "simplified" },
                { "label": "Project Style", "value": "style" },
                { "label": "Project Properties", "value": "properties" },
                { "label": "Project Insight", "value": "insight" }
            ]
        }
    ],
    "icon": "data:image/svg+xml;base64,"
}
