{
    "name": "appmixer.pdfco.core.MergePDF",
    "author": "Appmixer <info@appmixer.com>",
    "description": "Merges multiple PDFs or supported documents into a single PDF document.",
    "version": "1.0.0",
    "private": false,
    "auth": {
        "service": "appmixer:pdfco"
    },
    "quota": {
        "manager": "appmixer:pdfco",
        "resources": "requests"
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "files": {},
                    "name": {
                        "type": "string"
                    },
                    "async": {
                        "type": "boolean"
                    }
                },
                "required": [
                    "files"
                ]
            },
            "inspector": {
                "inputs": {
                    "files": {
                        "type": "expression",
                        "tooltip": "File URLs that need to be merged.",
                        "label": "Files",
                        "levels": [
                            "ADD"
                        ],
                        "fields": {
                            "files_item": {
                                "type": "text",
                                "label": "Files Item"
                            }
                        },
                        "index": 0
                    },
                    "name": {
                        "type": "text",
                        "tooltip": "The name for the resulting merged PDF file.",
                        "label": "Name",
                        "index": 1
                    },
                    "async": {
                        "type": "toggle",
                        "tooltip": "Set async to true for long processes to run in the background, API will then return a jobId which you can use with the Background Job Check component.",
                        "label": "Async",
                        "defaultValue": false,
                        "index": 2
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "options": [
                {
                    "label": "Url",
                    "value": "url",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Page Count",
                    "value": "pageCount",
                    "schema": {
                        "type": "number"
                    }
                },
                {
                    "label": "Error",
                    "value": "error",
                    "schema": {
                        "type": "boolean"
                    }
                },
                {
                    "label": "Status",
                    "value": "status",
                    "schema": {
                        "type": "number"
                    }
                },
                {
                    "label": "Credits",
                    "value": "credits",
                    "schema": {
                        "type": "number"
                    }
                },
                {
                    "label": "Remaining Credits",
                    "value": "remainingCredits",
                    "schema": {
                        "type": "number"
                    }
                },
                {
                    "label": "Duration",
                    "value": "duration",
                    "schema": {
                        "type": "number"
                    }
                },
                {
                    "label": "Name",
                    "value": "name",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Output Link Valid Till",
                    "value": "outputLinkValidTill",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Message",
                    "value": "message",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Job Id",
                    "value": "jobId",
                    "schema": {
                        "type": "string"
                    }
                }
            ]
        }
    ],
    "icon": "data:image/jpeg;base64,"
}