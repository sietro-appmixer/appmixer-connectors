{
    "name": "appmixer.klaviyo.campaign.UpdateCampaignSendJob",
    "label": "Update Campaign Send Job",
    "description": "Updates a campaign send job",
    "author": "Appmixer <info@appmixer.com>",
    "version": "1.0.0",
    "auth": {
        "service": "appmixer:klaviyo"
    },
    "quota": {
        "manager": "appmixer:klaviyo",
        "resources": "ten-requests"
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "campaignId": {
                        "type": "string"
                    },
                    "action": {
                        "type": "string"
                    }
                },
                "required": ["campaignId","action"]
            },
            "inspector": {
                "inputs": {
                    "campaignId": {
                        "type": "text",
                        "label": "Campaign ID",
                        "index": 0,
                        "tooltip": "ID of the campaign send job to update"
                    },
                    "action": {
                        "type": "select",
                        "label": "Action",
                        "index": 1,
                        "options": [
                            {
                                "label": "Cancel",
                                "value": "cancel"
                            },
                            {
                                "label": "Revert",
                                "value": "revert"
                            }
                        ]
                    }
                }
            }
        }
    ],
    "outPorts": ["out"],
    "icon": "data:image/png;base64,"
}
