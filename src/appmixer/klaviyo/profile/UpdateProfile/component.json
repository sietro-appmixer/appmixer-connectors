{
    "name": "appmixer.klaviyo.profile.UpdateProfile",
    "author": "Appmixer <info@appmixer.com>",
    "description": "Update attributes of an existing customer profile.",
    "version": "1.0.0",
    "private": false,
    "auth": {
        "service": "appmixer:klaviyo"
    },
    "quota": {
        "manager": "appmixer:klaviyo",
        "resources": "seventy-five-requests"
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "profileId": {
                        "type": "string"
                    },
                    "email": {
                        "type": "string"
                    },
                    "phoneNumber": {
                        "type": "string"
                    },
                    "externalId": {
                        "type": "string"
                    },
                    "firstName": {
                        "type": "string"
                    },
                    "lastName": {
                        "type": "string"
                    },
                    "organization": {
                        "type": "string"
                    },
                    "title": {
                        "type": "string"
                    },
                    "image": {
                        "type": "string"
                    },
                    "address1": {
                        "type": "string"
                    },
                    "address2": {
                        "type": "string"
                    },
                    "city": {
                        "type": "string"
                    },
                    "country": {
                        "type": "string"
                    },
                    "latitude": {
                        "type": "string"
                    },
                    "longitude": {
                        "type": "string"
                    },
                    "region": {
                        "type": "string"
                    },
                    "zip": {
                        "type": "string"
                    },
                    "timezone": {
                        "type": "string"
                    },
                    "ip": {
                        "type": "string"
                    }
                },
                "required": ["profileId"]
            },
            "inspector": {
                "inputs": {
                    "profileId": {
                        "type": "text",
                        "index": 0,
                        "label": "Profile ID",
                        "tooltip": "ID of the profile to update",
                        "source": {
                            "url": "/component/appmixer/klaviyo/profile/FindProfiles?outPort=out",
                            "data": {
                                "message": {
                                    "in/outputType": "array"
                                },
                                "properties": {
                                    "isSource": true
                                },
                                "transform": "./FindProfiles#toSelectArray"
                            }
                        }
                    },
                    "email": {
                        "type": "text",
                        "tooltip": "Email address of the profile. Either Email, Phone Number, or External ID is required.",
                        "label": "Email",
                        "index": 1,
                        "group": "base"
                    },
                    "phoneNumber": {
                        "type": "text",
                        "tooltip": "Phone number of the profile in E.164 format.",
                        "label": "Phone Number",
                        "index": 2,
                        "group": "base"
                    },
                    "externalId": {
                        "type": "text",
                        "tooltip": "A unique identifier used by customers to associate Klaviyo profiles with profiles in an external system, such as a point-of-sale system. Format varies based on the external system.",
                        "label": "External ID",
                        "index": 3,
                        "group": "base"
                    },
                    "firstName": {
                        "type": "text",
                        "tooltip": "First name of the customer.",
                        "label": "First Name",
                        "index": 4,
                        "group": "base"
                    },
                    "lastName": {
                        "type": "text",
                        "tooltip": "Last name of the customer.",
                        "label": "Last Name",
                        "index": 5,
                        "group": "base"
                    },
                    "organization": {
                        "type": "text",
                        "tooltip": "Name of the company or organization within the company for whom the individual works.",
                        "label": "Organization",
                        "index": 6,
                        "group": "base"
                    },
                    "title": {
                        "type": "text",
                        "tooltip": "Individual's job title.",
                        "label": "Title",
                        "index": 7,
                        "group": "base"
                    },
                    "image": {
                        "type": "text",
                        "tooltip": "URL pointing to the location of a profile image.",
                        "label": "Image",
                        "index": 8,
                        "group": "base"
                    },
                    "address1": {
                        "type": "text",
                        "label": "Address 1",
                        "index": 9,
                        "group": "location"
                    },
                    "address2": {
                        "type": "text",
                        "label": "Address 2",
                        "index": 10,
                        "group": "location"
                    },
                    "city": {
                        "type": "text",
                        "label": "City",
                        "index": 11,
                        "group": "location"
                    },
                    "country": {
                        "type": "text",
                        "label": "Country",
                        "index": 12,
                        "group": "location"
                    },
                    "latitude": {
                        "type": "text",
                        "tooltip": "Latitude coordinate. Precision of four decimal places is recommended.",
                        "label": "Latitude",
                        "index": 13,
                        "group": "location"
                    },
                    "longitude": {
                        "type": "text",
                        "tooltip": "Longitude coordinate. Precision of four decimal places is recommended.",
                        "label": "Longitude",
                        "index": 14,
                        "group": "location"
                    },
                    "region": {
                        "type": "text",
                        "tooltip": "Region within a country, such as state or province.",
                        "label": "Region",
                        "index": 15,
                        "group": "location"
                    },
                    "zip": {
                        "type": "text",
                        "label": "Zip Code",
                        "index": 16,
                        "group": "location"
                    },
                    "timezone": {
                        "type": "text",
                        "tooltip": "Time zone name. Using time zones from the IANA Time Zone Database is recommended.",
                        "label": "Timezone",
                        "index": 17,
                        "group": "location"
                    },
                    "ip": {
                        "type": "text",
                        "label": "IP Address",
                        "index": 18,
                        "group": "location"
                    }
                },
                "groups": {
                    "base": {
                        "label": "Base",
                        "index": 0
                    },
                    "location": {
                        "label": "Location",
                        "index": 1
                    }
                }
            }
        }
    ],
    "outPorts": ["out"],
    "icon": "data:image/png;base64,"
}
