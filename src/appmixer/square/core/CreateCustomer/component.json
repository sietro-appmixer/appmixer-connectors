                {
                    "label": "Phone Number",
                    "value": "phone_number",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Company Name",
                    "value": "company_name",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Preferences",
                    "value": "preferences",
                    "schema": {
                        "type": "object",
                        "properties": {
                            "email_unsubscribed": {
                                "type": "boolean",
                                "title": "Preferences.Email Unsubscribed"
                            }
                        }
                    }
                },
                {
                    "label": "Creation Source",
                    "value": "creation_source",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Version",
                    "value": "version",
                    "schema": {
                        "type": "number"
                    }
                }
            ]
        }
    ],
    "icon": "data:image/svg+xml;base64,"
