{
    "name": "appmixer.square.core.CreateCustomer",
    "author": "Appmixer <info@appmixer.com>",
    "description": "Create a new customer profile.",
    "version": "1.0.0",
    "private": false,
    "auth": {
        "service": "appmixer:square"
    },
    "quota": {
        "manager": "appmixer:square",
        "resources": "requests"
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "given_name": {
                        "type": "string"
                    },
                    "family_name": {
                        "type": "string"
                    },
                    "company_name": {
                        "type": "string"
                    },
                    "email_address": {
                        "type": "string"
                    },
                    "phone_number": {
                        "type": "string"
                    }
                },
                "anyOf": [
                    {
                        "required": [
                            "given_name"
                        ]
                    },
                    {
                        "required": [
                            "family_name"
                        ]
                    },
                    {
                        "required": [
                            "company_name"
                        ]
                    },
                    {
                        "required": [
                            "email_address"
                        ]
                    },
                    {
                        "required": [
                            "phone_number"
                        ]
                    }
                ]
            },
            "inspector": {
                "inputs": {
                    "given_name": {
                        "type": "text",
                        "tooltip": "The first name of the customer.",
                        "label": "Given Name",
                        "index": 0
                    },
                    "family_name": {
                        "type": "text",
                        "tooltip": "The last name of the customer.",
                        "label": "Family Name",
                        "index": 1
                    },
                    "company_name": {
                        "type": "text",
                        "tooltip": "The name of the customer's workplace.",
                        "label": "Company Name",
                        "index": 2
                    },
                    "email_address": {
                        "type": "text",
                        "tooltip": "The email address of the customer.",
                        "label": "Email Address",
                        "index": 3
                    },
                    "phone_number": {
                        "type": "text",
                        "tooltip": "The phone number of the customer.",
                        "label": "Phone Number",
                        "index": 4
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "options": [
                {
                    "label": "Customer Id",
                    "value": "id",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Created At",
                    "value": "created_at",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Updated At",
                    "value": "updated_at",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Given Name",
                    "value": "given_name",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Family Name",
                    "value": "family_name",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Nickname",
                    "value": "nickname",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Email Address",
                    "value": "email_address",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Address",
                    "value": "address",
                    "schema": {
                        "type": "object",
                        "properties": {
                            "address_line_1": {
                                "type": "string",
                                "title": "Address.Address Line 1"
                            },
                            "locality": {
                                "type": "string",
                                "title": "Address.Locality"
                            },
                            "administrative_district_level_1": {
                                "type": "string",
