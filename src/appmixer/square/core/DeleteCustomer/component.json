{
    "name": "appmixer.square.core.DeleteCustomer",
    "author": "Appmixer <info@appmixer.com>",
    "description": "Delete a customer profile from a business.",
    "version": "1.0.0",
    "private": false,
    "auth": {
        "service": "appmixer:square"
    },
    "quota": {
        "manager": "appmixer:square",
        "resources": "requests"
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "customer_id": {
                        "type": "string"
                    },
                    "version": {
                        "type": "integer"
                    }
                },
                "required": [
                    "customer_id"
                ]
            },
            "inspector": {
                "inputs": {
                    "customer_id": {
                        "type": "text",
                        "tooltip": "The ID of the customer to delete.",
                        "label": "Customer ID",
                        "index": 0
                    },
                    "version": {
                        "type": "number",
                        "tooltip": "The current version of the customer profile. Include this parameter to enable optimistic concurrency control.",
                        "label": "Customer Profile Version",
                        "index": 1
                    }
                }
            }
        }
    ],
    "outPorts": [
        "out"
    ],
    "icon": "data:image/svg+xml;base64,"
}