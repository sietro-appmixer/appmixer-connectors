{
    "name": "appmixer.square.core.GetCustomer",
    "author": "Appmixer <info@appmixer.com>",
    "description": "Retrieve detailed information for a specific customer by their Id.",
    "version": "1.0.0",
    "private": false,
    "auth": {
        "service": "appmixer:square"
    },
    "quota": {
        "manager": "appmixer:square",
        "resources": "requests"
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "customer_id": {
                        "type": "string"
                    }
                },
                "required": [
                    "customer_id"
                ]
            },
            "inspector": {
                "inputs": {
                    "customer_id": {
                        "type": "text",
                        "tooltip": "The unique identifier for the customer.",
                        "label": "Customer Id",
                        "index": 0
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "options": [
                {
                    "label": "Customer Id",
                    "value": "id",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Created At",
                    "value": "created_at",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Updated At",
                    "value": "updated_at",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Given Name",
                    "value": "given_name",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Family Name",
                    "value": "family_name",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Email Address",
                    "value": "email_address",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Address",
                    "value": "address",
                    "schema": {
                        "type": "object",
                        "properties": {
                            "address_line_1": {
                                "type": "string",
                                "title": "Address.Address Line 1"
                            },
                            "address_line_2": {
                                "type": "string",
                                "title": "Address.Address Line 2"
                            },
                            "locality": {
                                "type": "string",
                                "title": "Address.Locality"
                            },
                            "administrative_district_level_1": {
                                "type": "string",
                                "title": "Address.Administrative District Level 1"
                            },
                            "postal_code": {
                                "type": "string",
                                "title": "Address.Postal Code"
                            },
                            "country": {
                                "type": "string",
                                "title": "Address.Country"
                            }
                        }
                    }
                },
                {
                    "label": "Phone Number",
                    "value": "phone_number",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Reference Id",
                    "value": "reference_id",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Note",
                    "value": "note",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Preferences",
                    "value": "preferences",
                    "schema": {
                        "type": "object",
                        "properties": {
                            "email_unsubscribed": {
                                "type": "boolean",
                                "title": "Preferences.Email Unsubscribed"
                            }
                        }
                    }
                },
                {
                    "label": "Creation Source",
                    "value": "creation_source",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Group Ids",
                    "value": "group_ids",
                    "schema": {
                        "type": "array",
                        "items": {
                            "type": "string"
                        }
                    }
                },
                {
                    "label": "Segment Ids",
                    "value": "segment_ids",
                    "schema": {
                        "type": "array",
                        "items": {
                            "type": "string"
                        }
                    }
                },
                {
                    "label": "Version",
                    "value": "version",
                    "schema": {
                        "type": "number"
                    }
                }
            ]
        }
    ],
    "icon": "data:image/svg+xml;base64,"
