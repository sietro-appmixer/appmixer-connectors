{
    "name": "appmixer.square.core.UpdateCustomer",
    "author": "Appmixer <info@appmixer.com>",
    "description": "Update the profile information of an existing customer.",
    "version": "1.0.0",
    "private": false,
    "auth": {
        "service": "appmixer:square"
    },
    "quota": {
        "manager": "appmixer:square",
        "resources": "requests"
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "customer_id": {
                        "type": "string"
                    },
                    "given_name": {
                        "type": "string"
                    },
                    "family_name": {
                        "type": "string"
                    },
                    "email_address": {
                        "type": "string"
                    },
                    "phone_number": {
                        "type": "string"
                    }
                },
                "required": [
                    "customer_id"
                ]
            },
            "inspector": {
                "inputs": {
                    "customer_id": {
                        "type": "text",
                        "tooltip": "The unique identifier for the customer.",
                        "label": "Customer Id",
                        "index": 0
                    },
                    "given_name": {
                        "type": "text",
                        "tooltip": "Updated given name of the customer.",
                        "label": "Given Name",
                        "index": 1
                    },
                    "family_name": {
                        "type": "text",
                        "tooltip": "Updated family name of the customer.",
                        "label": "Family Name",
                        "index": 2
                    },
                    "email_address": {
                        "type": "text",
                        "tooltip": "Updated email address of the customer.",
                        "label": "Email Address",
                        "index": 3
                    },
                    "phone_number": {
                        "type": "text",
                        "tooltip": "Updated phone number of the customer.",
                        "label": "Phone Number",
                        "index": 4
                    }
                }
            }
        }
    ],
    "outPorts": ["out"],
    "icon": "data:image/svg+xml;base64,"
}