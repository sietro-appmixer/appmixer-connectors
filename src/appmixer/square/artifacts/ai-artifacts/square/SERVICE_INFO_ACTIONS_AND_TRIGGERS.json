                    },
                    "source_id": {
                        "type": "string",
                        "description": "A valid identifier for the source of funding for the payment (e.g., a card nonce)."
                    },
                    "customer_id": {
                        "type": "string",
                        "description": "The ID of the customer associated with the payment."
                    }
                }
            },
            "outputType": {
                "outputType": "object"
            }
