            "type": "action",
            "documentation": "https://developer.squareup.com/reference/square/payments-api/create-payment",
            "endpointUrl": "/v2/payments",
            "apiBaseUrl": "https://connect.squareup.com/",
            "method": "POST",
            "sampleResponse": {
                "payment": {
                    "id": "PAYMENT_ID",
