{
    "name": "appmixer.front.contacts.UpdateContact",
    "author": "Appmixer <info@appmixer.com>",
    "description": "Update an existing contact by ID.",
    "version": "1.0.0",
    "private": false,
    "auth": {
        "service": "appmixer:front"
    },
    "quota": {
        "manager": "appmixer:front",
        "resources": "requests"
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "string"
                    },
                    "name": {
                        "type": "string"
                    },
                    "description": {
                        "type": "string"
                    },
                    "links": {
                        "type": "string"
                    }
                },
                "required": ["id"]
            },
            "inspector": {
                "inputs": {
                    "id": {
                        "type": "text",
                        "label": "Contact ID",
                        "index": 0
                    },
                    "name": {
                        "type": "text",
                        "label": "Name",
                        "index": 1,
                        "tooltip": "Full name of the contact"
                    },
                    "description": {
                        "type": "textarea",
                        "label": "Description",
                        "index": 2,
                        "tooltip": "Notes about the contact"
                    },
                    "links": {
                        "type": "text",
                        "label": "Links",
                        "index": 3,
                        "tooltip": "Comma-separated list of links (URLs)"
                    }
                }
            }
        }
    ],
    "outPorts": ["out"],
    "icon": "data:image/png;base64,"
}
