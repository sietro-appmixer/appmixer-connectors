{
    "name": "appmixer.front.inbox.GetInbox",
    "author": "Appmixer <info@appmixer.com>",
    "description": "Fetch an inbox by its ID.",
    "version": "1.0.0",
    "auth": {
        "service": "appmixer:front"
    },
    "quota": {
        "manager": "appmixer:front",
        "resources": "requests"
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "inboxId": {
                        "type": "string"
                    }
                },
                "required": ["inboxId"]
            },
            "inspector": {
                "inputs": {
                    "inboxId": {
                        "type": "text",
                        "label": "Inbox ID",
                        "index": 0,
                        "tooltip": "The unique identifier of the inbox to fetch"
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "options": [
                { "label": "Inbox ID", "value": "id" },
                { "label": "Name", "value": "name" },
                { "label": "Type", "value": "type" },
                { "label": "Is Private", "value": "is_private" },
                { "label": "Is Public", "value": "is_public" },
                { "label": "Custom Fields", "value": "custom_fields", "schema": { "type": "object" } },
                { "label": "Custom Fields City", "value": "custom_fields.city" },
                { "label": "Custom Fields Is VIP", "value": "custom_fields.isVIP", "schema": { "type": "boolean" } },
                { "label": "Custom Fields Renewal Date", "value": "custom_fields.renewal_date", "schema": { "type": "number" } },
                { "label": "Custom Fields SLA Time", "value": "custom_fields.sla_time", "schema": { "type": "number" } },
                { "label": "Custom Fields Owner", "value": "custom_fields.owner" },
                { "label": "Custom Fields Reply To", "value": "custom_fields.replyTo" },
                { "label": "Custom Fields Job Title", "value": "custom_fields['Job Title']" },
                { "label": "Links", "value": "_links", "schema": { "type": "object" } },
                { "label": "Links Self", "value": "_links.self" },
                { "label": "Links Related", "value": "_links.related", "schema": { "type": "object" } },
                { "label": "Links Related Teammates", "value": "_links.related.teammates" },
                { "label": "Links Related Conversations", "value": "_links.related.conversations" },
                { "label": "Links Related Channels", "value": "_links.related.channels" },
                { "label": "Links Related Owner", "value": "_links.related.owner" }
            ]
        }
    ],
    "icon": "data:image/png;base64,"
}
