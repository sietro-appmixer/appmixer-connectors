{
    "name": "appmixer.strava.core.CreateManualActivity",
    "author": "Appmixer <info@appmixer.com>",
    "description": "Create a manual activity by providing details like name, type, start date, and elapsed time.",
    "version": "1.0.0",
    "private": false,
    "auth": {
        "service": "appmixer:strava"
    },
    "quota": {
        "manager": "appmixer:strava",
        "resources": "requests"
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "name": {
                        "type": "string"
                    },
                    "sportType": {
                        "type": "string"
                    },
                    "startDateLocal": {
                        "type": "string"
                    },
                    "elapsedTime": {
                        "type": "integer"
                    },
                    "description": {
                        "type": "string"
                    },
                    "distance": {
                        "type": "number"
                    },
                    "trainer": {
                        "type": "integer"
                    },
                    "commute": {
                        "type": "integer"
                    }
                },
                "required": ["name", "sportType", "startDateLocal", "elapsedTime"]
            },
            "inspector": {
                "inputs": {
                    "name": {
                        "type": "text",
                        "tooltip": "Name of the activity.",
                        "label": "Name",
                        "index": 1
                    },
                    "sportType": {
                        "type": "text",
                        "tooltip": "Sport type of activity (e.g., Run, MountainBikeRide, Ride).",
                        "label": "Sport Type",
                        "index": 2
                    },
                    "startDateLocal": {
                        "type": "text",
                        "tooltip": "Start date of the activity in ISO 8601 format (e.g., 2018-02-20T10:02:13Z).",
                        "label": "Start Date Local",
                        "index": 3
                    },
                    "elapsedTime": {
                        "type": "number",
                        "tooltip": "Elapsed time of the activity in seconds.",
                        "label": "Elapsed Time (seconds)",
                        "index": 4
                    },
                    "description": {
                        "type": "text",
                        "tooltip": "Description of the activity (optional).",
                        "label": "Description",
                        "index": 5
                    },
                    "distance": {
                        "type": "number",
                        "tooltip": "Distance of the activity in meters (optional).",
                        "label": "Distance (meters)",
                        "index": 6
                    },
                    "trainer": {
                        "type": "number",
                        "tooltip": "Set to 1 to mark as a trainer activity (optional).",
                        "label": "Trainer Activity",
                        "index": 7
                    },
                    "commute": {
                        "type": "number",
                        "tooltip": "Set to 1 to mark as a commute (optional).",
                        "label": "Commute",
                        "index": 8
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "options": [
                {
                    "label": "Activity ID",
                    "value": "id",
                    "schema": {
                        "type": "number"
                    }
                },
                {
                    "label": "Name",
                    "value": "name",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Distance",
                    "value": "distance",
                    "schema": {
                        "type": "number"
                    }
                },
                {
                    "label": "Moving Time",
                    "value": "moving_time",
                    "schema": {
                        "type": "number"
                    }
                },
                {
                    "label": "Elapsed Time",
                    "value": "elapsed_time",
                    "schema": {
                        "type": "number"
                    }
                },
                {
                    "label": "Type",
                    "value": "type",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Sport Type",
                    "value": "sport_type",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Start Date",
                    "value": "start_date",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Start Date Local",
                    "value": "start_date_local",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Timezone",
                    "value": "timezone",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Total Elevation Gain",
                    "value": "total_elevation_gain",
                    "schema": {
                        "type": "number"
                    }
                },
                {
                    "label": "Trainer",
                    "value": "trainer",
                    "schema": {
                        "type": "boolean"
                    }
                },
                {
                    "label": "Commute",
                    "value": "commute",
                    "schema": {
                        "type": "boolean"
                    }
                },
                {
                    "label": "Manual",
                    "value": "manual",
                    "schema": {
                        "type": "boolean"
                    }
                },
                {
                    "label": "Private",
                    "value": "private",
                    "schema": {
                        "type": "boolean"
                    }
                },
                {
                    "label": "Description",
                    "value": "description",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Resource State",
                    "value": "resource_state",
                    "schema": {
                        "type": "number"
                    }
                },
                {
                    "label": "Athlete",
                    "value": "athlete",
                    "schema": {
                        "type": "object",
                        "properties": {
                            "id": {
                                "type": "number"
                            },
                            "resource_state": {
                                "type": "number"
                            }
                        }
                    }
                }
            ]
        }
    ],
    "icon": "data:image/svg+xml;base64,"
}