{
    "name": "appmixer.intercom.core.CreateContact",
    "author": "Appmixer <info@appmixer.com>",
    "description": "Create a new contact with basic details and custom attributes.",
    "version": "1.0.0",
    "private": false,
    "auth": {
        "service": "appmixer:intercom"
    },
    "quota": {
        "manager": "appmixer:intercom",
        "resources": "requests"
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "email": {
                        "type": "string"
                    },
                    "name": {
                        "type": "string"
                    },
                    "external_id": {
                        "type": "string"
                    },
                    "role": {
                        "type": "string",
                        "enum": [
                            "user",
                            "lead"
                        ]
                    }
                },
                "anyOf": [
                    {
                        "required": [
                            "email"
                        ]
                    },
                    {
                        "required": [
                            "external_id"
                        ]
                    }
                ]
            },
            "inspector": {
                "inputs": {
                    "email": {
                        "type": "text",
                        "tooltip": "Email address of the contact",
                        "label": "Email",
                        "index": 0
                    },
                    "name": {
                        "type": "text",
                        "tooltip": "Full name of the contact",
                        "label": "Name",
                        "index": 1
                    },
                    "external_id": {
                        "type": "text",
                        "tooltip": "A unique identifier for the contact in your system",
                        "label": "External Id",
                        "index": 2
                    },
                    "role": {
                        "type": "select",
                        "tooltip": "Role of the contact in Intercom (user or lead)",
                        "label": "Role",
                        "index": 3,
                        "options": [
                            {
                                "label": "User",
                                "value": "user"
                            },
                            {
                                "label": "Lead",
                                "value": "lead"
                            }
                        ]
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "options": [
                {
                    "label": "Type",
                    "value": "type",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Contact Id",
                    "value": "id",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Workspace Id",
                    "value": "workspace_id",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Contact External Id",
                    "value": "external_id",
                    "schema": {
                        "type": "null"
                    }
                },
                {
                    "label": "Role",
                    "value": "role",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Email",
                    "value": "email",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Phone",
                    "value": "phone",
                    "schema": {
                        "type": "null"
                    }
                },
                {
                    "label": "Name",
                    "value": "name",
                    "schema": {
                        "type": "null"
                    }
                },
                {
                    "label": "Avatar",
                    "value": "avatar",
                    "schema": {
                        "type": "null"
                    }
                },
                {
                    "label": "Owner Id",
                    "value": "owner_id",
                    "schema": {
                        "type": "null"
                    }
                }
            ]
        }
    ],
    "icon": "data:image/svg+xml;base64,"
}