{
    "name": "appmixer.intercom.core.AddContactToCompany",
    "author": "Appmixer <info@appmixer.com>",
    "description": "Attach a Contact to a Company. Please note that the changes made using the Intercom API are not reflected in real time.",
    "version": "1.0.0",
    "auth": {
        "service": "appmixer:intercom"
    },
    "quota": {
        "manager": "appmixer:intercom",
        "resources": "requests"
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "contact_id": {
                        "type": "string"
                    },
                    "company_id": {
                        "type": "string"
                    }
                },
                "required": [
                    "contact_id",
                    "company_id"
                ]
            },
            "inspector": {
                "inputs": {
                    "contact_id": {
                        "type": "text",
                        "label": "Contact Id",
                        "tooltip": "The unique identifier for the contact which is given by Intercom",
                        "index": 1
                    },
                    "company_id": {
                        "type": "text",
                        "label": "Company Id",
                        "tooltip": "The unique identifier for the company which is given by Intercom",
                        "index": 2
                    }
                }
            }
        }
    ],
    "outPorts": [
        "out"
    ],
    "icon": "data:image/svg+xml;base64,"
}