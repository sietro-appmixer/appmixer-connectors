{
    "name": "appmixer.mcpservers.webscraping_ai_mcp.MCPServer",
    "label": "MCP - WebScraping.AI (MCP Server)",
    "author": "Appmixer <info@appmixer.com>",
    "description": "WebScraping.AI for web data extraction capabilities. (webscraping-ai-mcp 1.0.2). Documentation: <a target=_blank href='https://github.com/webscraping-ai/webscraping-ai-mcp-server'>https://github.com/webscraping-ai/webscraping-ai-mcp-server</a>",
    "auth": { "service": "appmixer:mcpservers:webscraping_ai_mcp" },
    "properties": {
        "schema": {
            "type": "object",
            "properties": {
                "tools": { "type": ["array", "string"] }
            }
        },
        "inspector": {
            "inputs": {
                "tools": {
                    "label": "Tools",
                    "type": "multiselect",
                    "tooltip": "Leave as is if you want to use all tools. Otherwise, select the tools you want to use.",
                    "index": 1,
                    "source": {
                        "url": "/component/appmixer/mcpservers/webscraping_ai_mcp/ListTools?outPort=out",
                        "data": {
                            "transform": "./ListTools#toolsToSelectArray"
                        }
                    }
                }
            }
        }
    },
    "inPorts": [{
        "name": "in"
    }],
    "outPorts": [{
        "name": "out",
        "options": [{
            "label": "Output",
            "value": "output"
        }]
    }],
    "icon": "data:image/png;base64,"
}
