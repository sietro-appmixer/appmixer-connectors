{
    "name": "appmixer.mcpservers.upstash_mcp_server.CallTool",
    "author": "Appmixer <info@appmixer.com>",
    "description": "Call a single tool.",
    "auth": { "service": "appmixer:mcpservers:upstash_mcp_server" },
    "properties": {
        "schema": {
            "properties": {
                "tool": { "type": "string" }
            }
        },
        "inspector": {
            "inputs": {
                "tool": {
                    "type": "select",
                    "label": "Tool",
                    "index": 1,
                    "tooltip": "The tool to call.",
                    "source": {
                        "url": "/component/appmixer/mcpservers/upstash_mcp_server/ListTools?outPort=out",
                        "data": {
                            "transform": "./ListTools#toolsToSelectArray"
                        }
                    }
                }
            }
        }
    },
    "inPorts": [{
        "name": "in",
        "schema": {
            "type": "object"
        },
        "source": {
            "url": "/component/appmixer/mcpservers/upstash_mcp_server/CallTool?outPort=out",
            "data": {
                "properties": {
                    "tool": "properties/tool",
                    "generateInspector": true
                }
            }
        }
    }],
    "outPorts": [{
        "name": "out",
        "options": [{
            "label": "Output",
            "value": "output"
        }]
    }],
    "icon": "data:image/svg+xml;base64,"
}
