{
    "name": "appmixer.mailerlite.core.CreateCampaign",
    "description": "Create a new email campaign with specified content and settings.",
    "author": "Appmixer <info@appmixer.com>",
    "version": "1.0.0",
    "auth": {
        "service": "appmixer:mailerlite"
    },
    "quota": {
        "manager": "appmixer:mailerlite",
        "resources": "requests"
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "name": {
                        "type": "string"
                    },
                    "subject": {
                        "type": "string"
                    },
                    "senderName": {
                        "type": "string"
                    },
                    "fromAddress": {
                        "type": "string",
                        "format": "email"
                    },
                    "content": {
                        "type": "string"
                    },
                    "groups": {
                    }
                },
                "required": ["name", "subject", "senderName", "fromAddress", "content"]
            },
            "inspector": {
                "inputs": {
                    "name": {
                        "type": "text",
                        "tooltip": "Name of the campaign for internal identification.",
                        "label": "Campaign Name",
                        "index": 0
                    },
                    "subject": {
                        "type": "text",
                        "tooltip": "Subject line of the email that recipients will see.",
                        "label": "Email Subject",
                        "index": 1
                    },
                    "senderName": {
                        "type": "text",
                        "tooltip": "Name that will appear as the sender of the email.",
                        "label": "Email Sender Name",
                        "index": 2
                    },
                    "fromAddress": {
                        "type": "text",
                        "tooltip": "Email address that the email will be sent from.",
                        "label": "Email From Address",
                        "index": 3
                    },
                    "content": {
                        "type": "textarea",
                        "tooltip": "HTML content of the email campaign.",
                        "label": "Email Content",
                        "index": 5
                    },
                    "groups": {
                        "type": "expression",
                        "tooltip": "Array of group IDs to which the campaign will be sent.",
                        "label": "Groups",
                        "levels": [
                            "AND"
                        ],
                        "fields": {
                            "groups_item": {
                                "type": "text",
                                "label": "Groups Item"
                            }
                        },
                        "index": 4
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "options": [
                {
                    "label": "Camnpaign Id",
                    "value": "id",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Name",
                    "value": "name",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Subject",
                    "value": "subject",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Status",
                    "value": "status",
                    "schema": {
                        "type": "string"
                    }
                }
            ]
        }
    ],
    "icon": "data:image/jpeg;base64,"
}