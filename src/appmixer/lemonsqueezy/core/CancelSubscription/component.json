{
    "name": "appmixer.lemonsqueezy.core.CancelSubscription",
    "label": "Cancel Subscription",
    "description": "Cancel an active subscription. This will prevent future renewal charges.",
    "author": "Appmixer <info@appmixer.com>",
    "version": "1.0.0",
    "auth": {
        "service": "appmixer:lemonsqueezy"
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "subscriptionId": {
                        "type": "string",
                        "title": "Subscription ID",
                        "description": "The ID of the subscription to cancel"
                    }
                },
                "required": ["subscriptionId"]
            },
            "inspector": {
                "inputs": {
                    "subscriptionId": {
                        "type": "text",
                        "label": "Subscription ID",
                        "index": 1,
                        "tooltip": "Enter the ID of the subscription you want to cancel"
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "options": [
                { "label": "Subscription ID", "value": "id" },
                { "label": "Store ID", "value": "storeId" },
                { "label": "Customer ID", "value": "customerId" },
                { "label": "Order ID", "value": "orderId" },
                { "label": "Product ID", "value": "productId" },
                { "label": "Variant ID", "value": "variantId" },
                { "label": "Status", "value": "status" },
                { "label": "Cancelled", "value": "cancelled" },
                { "label": "Created At", "value": "createdAt" },
                { "label": "Updated At", "value": "updatedAt" },
                { "label": "Ends At", "value": "endsAt" },
                { "label": "Renews At", "value": "renewsAt" }
            ]
        }
    ],
    "icon": "data:image/svg+xml;base64,"
}
