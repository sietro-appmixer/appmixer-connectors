{
    "name": "appmixer.sonarqube.core.ShowFileDuplications",
    "label": "Show File Duplications",
    "author": "Appmixer <info@appmixer.com>",
    "description": "Retrieves duplication details for a specified file in a project. Requires appropriate browse permissions on the file's project.",
    "version": "1.0.0",
    "private": false,
    "auth": {
        "service": "appmixer:sonarqube"
    },
    "quota": {
        "manager": "appmixer:sonarqube",
        "resources": "requests"
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "key": {
                        "type": "string"
                    }
                },
                "required": ["key"]
            },
            "inspector": {
                "inputs": {
                    "key": {
                        "type": "text",
                        "tooltip": "File key (required). Example: my_project:/src/foo/Bar.php",
                        "label": "Key",
                        "index": 0
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "options": [
                {
                    "label": "Duplications",
                    "value": "duplications",
                    "schema": {
                        "type": "array",
                        "items": {
                            "type": "object",
                            "properties": {
                                "blocks": {
                                    "type": "array",
                                    "items": {
                                        "type": "object",
                                        "properties": {
                                            "from": {
                                                "type": "number",
                                                "title": "Duplications.Blocks.From"
                                            },
                                            "size": {
                                                "type": "number",
                                                "title": "Duplications.Blocks.Size"
                                            },
                                            "_ref": {
                                                "type": "string",
                                                "title": "Duplications.Blocks.Ref"
                                            }
                                        }
                                    },
                                    "title": "Duplications.Blocks"
                                }
                            }
                        }
                    }
                },
                {
                    "label": "Files",
                    "value": "files",
                    "schema": {
                        "type": "object",
                        "properties": {
                            "1": {
                                "type": "object",
                                "properties": {
                                    "key": {
                                        "type": "string",
                                        "title": "Files.1.Key"
                                    },
                                    "name": {
                                        "type": "string",
                                        "title": "Files.1.Name"
                                    },
                                    "projectName": {
                                        "type": "string",
                                        "title": "Files.1.Project Name"
                                    }
                                },
                                "title": "Files.1"
                            }
                        }
                    }
                }
            ]
        }
    ],
    "icon": "data:image/svg+xml;base64,"
}
