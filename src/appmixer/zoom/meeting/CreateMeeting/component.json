{
    "name": "appmixer.zoom.meeting.CreateMeeting",
    "author": "Appmixer <info@appmixer.com>",
    "description": "Create a new meeting for a user.",
    "version": "1.0.0",
    "private": false,
    "auth": {
        "service": "appmixer:zoom",
        "scope": [
            "meeting:write:meeting"
        ]
    },
    "quota": {
        "manager": "appmixer:zoom",
        "resources": "requests"
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "topic": {
                        "type": "string"
                    },
                    "startTime": {
                        "type": "string"
                    },
                    "duration": {
                        "type": "number"
                    },
                    "password": {
                        "type": "string"
                    },
                    "agenda": {
                        "type": "string"
                    }
                }
            },
            "inspector": {
                "inputs": {
                    "topic": {
                        "type": "text",
                        "tooltip": "The topic or title of the meeting",
                        "label": "Topic",
                        "index": 0
                    },
                    "startTime": {
                        "type": "date-time",
                        "tooltip": "The start time of the meeting",
                        "label": "Start Time",
                        "index": 1
                    },
                    "duration": {
                        "type": "number",
                        "tooltip": "The duration of the meeting in minutes",
                        "label": "Duration",
                        "index": 2
                    },
                    "password": {
                        "type": "text",
                        "tooltip": "The password required to join the meeting",
                        "label": "Password",
                        "index": 3
                    },
                    "agenda": {
                        "type": "text",
                        "tooltip": "The agenda or a brief description of the meeting",
                        "label": "Agenda",
                        "index": 4
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "options": [
                {
                    "label": "Meeting ID",
                    "value": "id",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Topic",
                    "value": "topic",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Start Time",
                    "value": "start_time",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Join Url",
                    "value": "join_url",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Start Url",
                    "value": "start_url",
                    "schema": {
                        "type": "string"
                    }
                }
            ]
        }
    ],
    "icon": "data:image/png;base64,"
}
