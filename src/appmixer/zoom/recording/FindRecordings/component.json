{
    "name": "appmixer.zoom.recording.FindRecordings",
    "author": "Appmixer <info@appmixer.com>",
    "description": "Retrieve a list of meeting recordings for a specified user within a given time frame.",
    "version": "1.0.0",
    "private": false,
    "auth": {
        "service": "appmixer:zoom",
        "scope": [
            "cloud_recording:read:list_user_recordings"
        ]
    },
    "quota": {
        "manager": "appmixer:zoom",
        "resources": "requests"
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "from": {
                        "type": "string"
                    },
                    "to": {
                        "type": "string"
                    },
                    "outputType": {
                        "type": "string"
                    }
                }
            },
            "inspector": {
                "inputs": {
                    "from": {
                        "type": "date-time",
                        "tooltip": "Start date for the recordings search (YYYY-MM-DD)",
                        "label": "From",
                        "index": 0,
                        "config": {
                            "enableTime": false,
                            "format": "YYYY-MM-DD"
                        }
                    },
                    "to": {
                        "type": "date-time",
                        "tooltip": "End date for the recordings search (YYYY-MM-DD)",
                        "label": "To",
                        "index": 1,
                        "config": {
                            "enableTime": false,
                            "format": "YYYY-MM-DD"
                        }
                    },
                    "outputType": {
                        "type": "select",
                        "label": "Output Type",
                        "index": 2,
                        "defaultValue": "array",
                        "tooltip": "Choose whether you want to receive the result set as one complete list, or first item only or one item at a time or stream the items to a file.",
                        "options": [
                            {
                                "label": "First Item Only",
                                "value": "first"
                            },
                            {
                                "label": "All items at once",
                                "value": "array"
                            },
                            {
                                "label": "One item at a time",
                                "value": "object"
                            },
                            {
                                "label": "Store to CSV file",
                                "value": "file"
                            }
                        ]
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "source": {
                "url": "/component/appmixer/zoom/recording/FindRecordings?outPort=out",
                "data": {
                    "properties": {
                        "generateOutputPortOptions": true
                    },
                    "messages": {
                        "in/outputType": "inputs/in/outputType"
                    }
                }
            }
        },
        {
            "name": "notFound"
        }
    ],
    "icon": "data:image/png;base64,"
}
