{
    "name": "appmixer.vercel.core.CreateProject",
    "label": "Create Project",
    "author": "Appmixer <info@appmixer.com>",
    "description": "Create a new project with the specified configuration details.",
    "version": "1.0.0",
    "private": false,
    "auth": {
        "service": "appmixer:vercel"
    },
    "quota": {
        "manager": "appmixer:vercel",
        "resources": "requests"
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "name": {
                        "type": "string"
                    },
                    "accountId": {
                        "type": "string"
                    },
                    "framework": {
                        "type": "string"
                    },
                    "devCommand": {
                        "type": "string"
                    },
                    "buildCommand": {
                        "type": "string"
                    },
                    "outputDirectory": {
                        "type": "string"
                    },
                    "publicSource": {
                        "type": "boolean"
                    },
                    "teamId": {
                        "type": "string"
                    }
                }
            },
            "inspector": {
                "inputs": {
                    "name": {
                        "type": "text",
                        "tooltip": "Name of the project.",
                        "label": "Name",
                        "index": 0
                    },
                    "accountId": {
                        "type": "text",
                        "tooltip": "Account identifier associated with the project.",
                        "label": "Account Id",
                        "index": 1
                    },
                    "framework": {
                        "type": "text",
                        "tooltip": "Framework to be used (e.g., blitzjs, nextjs, gatsby, remix, react-router, astro, etc.).",
                        "label": "Framework",
                        "index": 2
                    },
                    "devCommand": {
                        "type": "text",
                        "tooltip": "Command to start the development environment.",
                        "label": "Dev Command",
                        "index": 3
                    },
                    "buildCommand": {
                        "type": "text",
                        "tooltip": "Command to build the project.",
                        "label": "Build Command",
                        "index": 4
                    },
                    "outputDirectory": {
                        "type": "text",
                        "tooltip": "Directory where the build output will be placed.",
                        "label": "Output Directory",
                        "index": 5
                    },
                    "publicSource": {
                        "type": "toggle",
                        "tooltip": "Flag to define if the project source should be public.",
                        "label": "Public Source",
                        "index": 6,
                        "defaultValue": false
                    },
                    "teamId": {
                        "type": "text",
                        "tooltip": "Optional team identifier if creating a project for a team.",
                        "label": "Team Id",
                        "index": 7
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "options": [
                {
                    "label": "Id",
                    "value": "id",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Name",
                    "value": "name",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Framework",
                    "value": "framework",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Dev Command",
                    "value": "devCommand",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Build Command",
                    "value": "buildCommand",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Output Directory",
                    "value": "outputDirectory",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Public Source",
                    "value": "publicSource",
                    "schema": {
                        "type": "boolean"
                    }
                },
                {
                    "label": "Created At",
                    "value": "createdAt",
                    "schema": {
                        "type": "number"
                    }
                },
                {
                    "label": "Updated At",
                    "value": "updatedAt",
                    "schema": {
                        "type": "number"
                    }
                }
            ]
        }
    ],
    "icon": "data:image/svg+xml;base64,"
}
