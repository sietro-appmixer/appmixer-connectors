{
    "name": "appmixer.vercel.core.FindDeployments",
    "label": "Find Deployments",
    "author": "Appmixer <info@appmixer.com>",
    "description": "Retrieve all deployments with options to filter by project, status or target.",
    "version": "1.0.0",
    "private": false,
    "auth": {
        "service": "appmixer:vercel"
    },
    "quota": {
        "manager": "appmixer:vercel",
        "resources": "requests"
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "projectId": {
                        "type": "string"
                    },
                    "state": {
                        "type": "string"
                    },
                    "target": {
                        "type": "string"
                    },
                    "teamId": {
                        "type": "string"
                    },
                    "since": {
                        "type": "string",
                        "oneOf": [{ "format": "date-time" }, { "format": "date" }]
                    },
                    "until": {
                        "type": "string",
                        "oneOf": [{ "format": "date-time" }, { "format": "date" }]
                    },
                    "outputType": {
                        "type": "string"
                    }
                }
            },
            "inspector": {
                "inputs": {
                    "projectId": {
                        "type": "text",
                        "tooltip": "Filter deployments by the associated project identifier or name.",
                        "label": "Project Id",
                        "index": 0
                    },
                    "state": {
                        "type": "text",
                        "tooltip": "Filter deployments by status (e.g., READY,BUILDING,ERROR).",
                        "label": "State",
                        "index": 1
                    },
                    "target": {
                        "type": "text",
                        "tooltip": "Filter by deployment target (production or preview).",
                        "label": "Target",
                        "index": 2
                    },
                    "teamId": {
                        "type": "text",
                        "tooltip": "Optional team identifier if filtering team deployments.",
                        "label": "Team Id",
                        "index": 3
                    },
                    "since": {
                        "type": "date-time",
                        "tooltip": "Get deployments created after this date and time. Will be converted to a JavaScript timestamp (milliseconds).",
                        "label": "Since Timestamp",
                        "index": 4
                    },
                    "until": {
                        "type": "date-time",
                        "tooltip": "Get deployments created before this date and time. Will be converted to a JavaScript timestamp (milliseconds).",
                        "label": "Until Timestamp",
                        "index": 5
                    },
                    "outputType": {
                        "type": "select",
                        "label": "Output Type",
                        "index": 6,
                        "defaultValue": "array",
                        "tooltip": "Choose whether you want to receive the result set as one complete list, or first item only or one item at a time or stream the items to a file.",
                        "options": [
                            {
                                "label": "First Item Only",
                                "value": "first"
                            },
                            {
                                "label": "All items at once",
                                "value": "array"
                            },
                            {
                                "label": "One item at a time",
                                "value": "object"
                            },
                            {
                                "label": "Store to CSV file",
                                "value": "file"
                            }
                        ]
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "source": {
                "url": "/component/appmixer/vercel/core/FindDeployments?outPort=out",
                "data": {
                    "properties": {
                        "generateOutputPortOptions": true
                    },
                    "messages": {
                        "in/outputType": "inputs/in/outputType"
                    }
                }
            }
        },
        {
            "name": "notFound"
        }
    ],
    "icon": "data:image/svg+xml;base64,"
}
