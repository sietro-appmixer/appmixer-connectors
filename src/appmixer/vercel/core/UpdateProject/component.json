{
    "name": "appmixer.vercel.core.UpdateProject",
    "label": "Update Project",
    "author": "Appmixer <info@appmixer.com>",
    "description": "Modify settings of an existing project.",
    "version": "1.0.0",
    "private": false,
    "auth": {
        "service": "appmixer:vercel"
    },
    "quota": {
        "manager": "appmixer:vercel",
        "resources": "requests"
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "string"
                    },
                    "name": {
                        "type": "string"
                    },
                    "devCommand": {
                        "type": "string"
                    },
                    "buildCommand": {
                        "type": "string"
                    },
                    "outputDirectory": {
                        "type": "string"
                    },
                    "publicSource": {
                        "type": "boolean"
                    },
                    "teamId": {
                        "type": "string"
                    }
                },
                "required": ["id"]
            },
            "inspector": {
                "inputs": {
                    "id": {
                        "type": "text",
                        "tooltip": "Unique identifier of the project to update.",
                        "label": "Id",
                        "index": 0
                    },
                    "name": {
                        "type": "text",
                        "tooltip": "New name for the project.",
                        "label": "Name",
                        "index": 1
                    },
                    "devCommand": {
                        "type": "text",
                        "tooltip": "Updated development command.",
                        "label": "Dev Command",
                        "index": 2
                    },
                    "buildCommand": {
                        "type": "text",
                        "tooltip": "Updated build command.",
                        "label": "Build Command",
                        "index": 3
                    },
                    "outputDirectory": {
                        "type": "text",
                        "tooltip": "Updated output directory.",
                        "label": "Output Directory",
                        "index": 4
                    },
                    "publicSource": {
                        "type": "toggle",
                        "tooltip": "Updated public access flag.",
                        "label": "Public Source",
                        "index": 5
                    },
                    "teamId": {
                        "type": "text",
                        "tooltip": "Optional team identifier if updating a team project.",
                        "label": "Team Id",
                        "index": 6
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "options": [
                {
                    "label": "Id",
                    "value": "id",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Name",
                    "value": "name",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Framework",
                    "value": "framework",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Dev Command",
                    "value": "devCommand",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Build Command",
                    "value": "buildCommand",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Output Directory",
                    "value": "outputDirectory",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Public Source",
                    "value": "publicSource",
                    "schema": {
                        "type": "boolean"
                    }
                },
                {
                    "label": "Updated At",
                    "value": "updatedAt",
                    "schema": {
                        "type": "number"
                    }
                }
            ]
        }
    ],
    "icon": "data:image/svg+xml;base64,"
}
