{
    "name": "appmixer.vercel.core.CancelDeployment",
    "label": "Cancel Deployment",
    "author": "Appmixer <info@appmixer.com>",
    "description": "Cancel a running deployment using its unique identifier.",
    "version": "1.0.0",
    "private": false,
    "auth": {
        "service": "appmixer:vercel"
    },
    "quota": {
        "manager": "appmixer:vercel",
        "resources": "requests"
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "string"
                    },
                    "teamId": {
                        "type": "string"
                    }
                }
            },
            "inspector": {
                "inputs": {
                    "id": {
                        "type": "text",
                        "tooltip": "Unique identifier of the deployment to cancel.",
                        "label": "Id",
                        "index": 0
                    },
                    "teamId": {
                        "type": "text",
                        "tooltip": "Optional team identifier if the deployment belongs to a team.",
                        "label": "Team Id",
                        "index": 1
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "options": [
                {
                    "label": "Uid",
                    "value": "uid",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Status",
                    "value": "status",
                    "schema": {
                        "type": "string"
                    }
                }
            ]
        }
    ],
    "icon": "data:image/svg+xml;base64,"
}
