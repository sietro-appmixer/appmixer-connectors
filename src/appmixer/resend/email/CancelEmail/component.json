{
    "name": "appmixer.resend.email.CancelEmail",
    "description": "Cancel a scheduled email.",
    "author": "Appmixer <info@appmixer.com>",
    "version": "1.0.0",
    "auth": {
        "service": "appmixer:resend"
    },
    "quota": {
        "manager": "appmixer:resend",
        "resources": "requests",
        "maxWait": 2000,
        "scope": {
            "userId": "{{userId}}"
        }
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "string",
                        "title": "Email ID",
                        "description": "The unique identifier of the email to cancel."
                    }
                },
                "required": [
                    "id"
                ]
            },
            "inspector": {
                "inputs": {
                    "id": {
                        "type": "text",
                        "tooltip": "The unique identifier of the email to cancel.",
                        "label": "Email ID",
                        "index": 0
                    }
                }
            }
        }
    ],
    "outPorts": [
        "out"
    ],
    "icon": "data:image/svg+xml;base64,"
}
