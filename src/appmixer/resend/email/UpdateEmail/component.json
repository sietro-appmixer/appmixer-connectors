{
    "name": "appmixer.resend.email.UpdateEmail",
    "description": "Update a scheduled email.",
    "author": "Appmixer <info@appmixer.com>",
    "version": "1.0.0",
    "auth": {
        "service": "appmixer:resend"
    },
    "quota": {
        "manager": "appmixer:resend",
        "resources": "requests",
        "maxWait": 2000,
        "scope": {
            "userId": "{{userId}}"
        }
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "string",
                        "title": "Email ID",
                        "description": "The unique identifier of the email to update."
                    },
                    "scheduled_at": {
                        "type": "string",
                        "title": "Scheduled At",
                        "description": "Schedule email to be sent later. The date should be in ISO 8601 format (e.g: 2024-08-05T11:52:01.858Z)."
                    }
                },
                "required": [
                    "id"
                ]
            },
            "inspector": {
                "inputs": {
                    "id": {
                        "type": "text",
                        "tooltip": "The unique identifier of the email to update.",
                        "label": "Email ID",
                        "index": 0
                    },
                    "scheduled_at": {
                        "type": "text",
                        "tooltip": "Schedule email to be sent later. The date should be in ISO 8601 format (e.g: 2024-08-05T11:52:01.858Z).",
                        "label": "Scheduled At",
                        "index": 1
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "options": [
                {
                    "label": "Object",
                    "value": "object",
                    "schema": {
                        "type": "string",
                        "title": "Object Type"
                    }
                },
                {
                    "label": "ID",
                    "value": "id",
                    "schema": {
                        "type": "string",
                        "title": "Email ID"
                    }
                }
            ]
        }
    ],
    "icon": "data:image/svg+xml;base64,"
}
