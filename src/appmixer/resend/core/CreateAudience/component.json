{
	"name": "appmixer.resend.core.CreateAudience",
	"description": "Create a list of contacts.",
	"author": "Appmixer <info@appmixer.com>",
	"version": "1.0.0",
	"auth": {
		"service": "appmixer:resend"
	},
	"quota": {
		"manager": "appmixer:resend",
		"resources": "requests",
		"maxWait": 2000,
		"scope": {
			"userId": "{{userId}}"
		}
	},
	"inPorts": [
		{
			"name": "in",
			"schema": {
				"type": "object",
				"properties": {
					"name": {
						"type": "string",
						"title": "Name",
						"description": "The name of the audience you want to create."
					}
				},
				"required": [
					"name"
				]
			},
			"inspector": {
				"inputs": {
					"name": {
						"type": "text",
						"index": 1,
						"tooltip": "The name of the audience you want to create.",
						"label": "Audience Name"
					}
				}
			}
		}
	],
	"outPorts": [
		{
			"name": "out",
			"options": [
				{
					"label": "Object",
					"value": "object",
					"schema": {
						"type": "string"
					}
				},
				{
					"label": "ID",
					"value": "id",
					"schema": {
						"type": "string"
					}
				},
				{
					"label": "Name",
					"value": "name",
					"schema": {
						"type": "string"
					}
				}
			]
		}
	],
	"icon": "data:image/svg+xml;base64,"
}
