{
	"name": "appmixer.resend.core.GetContact",
	"description": "Retrieve a single contact from an audience.",
	"author": "Appmixer <info@appmixer.com>",
	"version": "1.0.0",
	"auth": {
		"service": "appmixer:resend"
	},
	"quota": {
		"manager": "appmixer:resend",
		"resources": "requests",
		"maxWait": 2000,
		"scope": {
			"userId": "{{userId}}"
		}
	},
	"inPorts": [
		{
			"name": "in",
			"schema": {
				"type": "object",
				"properties": {
					"audience_id": {
						"type": "string",
						"title": "Audience ID",
						"description": "The Audience ID."
					},
					"id": {
						"type": "string",
						"title": "Contact ID",
						"description": "The Contact ID (either id or email must be provided)."
					},
					"email": {
						"type": "string",
						"title": "Email",
						"description": "The Contact email (either id or email must be provided)."
					}
				},
				"required": [
					"audience_id"
				]
			},
			"inspector": {
				"inputs": {
					"audience_id": {
						"type": "select",
						"index": 1,
						"label": "Audience",
						"tooltip": "Select an audience.",
						"source": {
							"url": "/component/appmixer/resend/core/ListAudiences?outPort=out",
							"data": {
								"messages": {
									"in/outputType": "array",
									"in/isSource": true
								},
								"transform": "./ListAudiences#toSelectArray"
							}
						}
					},
					"id": {
						"type": "text",
						"index": 2,
						"tooltip": "The Contact ID (either id or email must be provided).",
						"label": "Contact ID"
					},
					"email": {
						"type": "text",
						"index": 3,
						"tooltip": "The Contact email (either id or email must be provided).",
						"label": "Email"
					}
				}
			}
		}
	],
	"outPorts": [
		{
			"name": "out",
			"options": [
				{
					"label": "Object",
					"value": "object",
					"schema": {
						"type": "string"
					}
				},
				{
					"label": "ID",
					"value": "id",
					"schema": {
						"type": "string"
					}
				},
				{
					"label": "Email",
					"value": "email",
					"schema": {
						"type": "string"
					}
				},
				{
					"label": "First Name",
					"value": "first_name",
					"schema": {
						"type": "string"
					}
				},
				{
					"label": "Last Name",
					"value": "last_name",
					"schema": {
						"type": "string"
					}
				},
				{
					"label": "Created At",
					"value": "created_at",
					"schema": {
						"type": "string"
					}
				},
				{
					"label": "Unsubscribed",
					"value": "unsubscribed",
					"schema": {
						"type": "boolean"
					}
				}
			]
		}
	],
	"icon": "data:image/svg+xml;base64,"
}
