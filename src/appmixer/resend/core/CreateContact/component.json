{
	"name": "appmixer.resend.core.CreateContact",
	"description": "Create a contact inside an audience.",
	"author": "Appmixer <info@appmixer.com>",
	"version": "1.0.0",
	"auth": {
		"service": "appmixer:resend"
	},
	"quota": {
		"manager": "appmixer:resend",
		"resources": "requests",
		"maxWait": 2000,
		"scope": {
			"userId": "{{userId}}"
		}
	},
	"inPorts": [
		{
			"name": "in",
			"schema": {
				"properties": {
					"audience_id": {
						"type": "string"
					},
					"email": {
						"type": "string"
					},
					"first_name": {
						"type": "string"
					},
					"last_name": {
						"type": "string"
					},
					"unsubscribed": {
						"type": "boolean"
					}
				},
				"required": [
					"audience_id",
					"email"
				]
			},
			"inspector": {
				"inputs": {
					"audience_id": {
						"type": "select",
						"index": 1,
						"label": "Audience",
						"tooltip": "Select an audience.",
						"source": {
							"url": "/component/appmixer/resend/core/ListAudiences?outPort=out",
							"data": {
								"messages": {
									"in/outputType": "array",
									"in/isSource": true
								},
								"transform": "./ListAudiences#toSelectArray"
							}
						}
					},
					"email": {
						"type": "text",
						"index": 2,
						"label": "Email",
						"tooltip": "The email address of the contact."
					},
					"first_name": {
						"type": "text",
						"index": 3,
						"label": "First Name",
						"tooltip": "The first name of the contact."
					},
					"last_name": {
						"type": "text",
						"index": 4,
						"label": "Last Name",
						"tooltip": "The last name of the contact."
					},
					"unsubscribed": {
						"type": "toggle",
						"defaultValue": false,
						"index": 5,
						"label": "Unsubscribed",
						"tooltip": "The subscription status."
					}
				}
			}
		}
	],
	"outPorts": [
		{
			"name": "out",
			"options": [
				{
					"label": "Contact ID",
					"value": "id",
					"schema": {
						"type": "string"
					}
				},
				{
					"label": "Object Type",
					"value": "object",
					"schema": {
						"type": "string"
					}
				}
			]
		}
	],
	"icon": "data:image/svg+xml;base64,"
}
