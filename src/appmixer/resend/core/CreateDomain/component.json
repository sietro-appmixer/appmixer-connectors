{
    "name": "appmixer.resend.core.CreateDomain",
    "author": "Appmixer <info@appmixer.com>",
    "description": "Add a new domain for sending emails. Requires an API Key in the Authorization header as a Bearer token.",
    "version": "1.0.0",
    "private": false,
    "auth": {
        "service": "appmixer:resend"
    },
    "quota": {
        "manager": "appmixer:resend",
        "resources": "requests",
        "maxWait": 2000,
        "scope": {
            "userId": "{{userId}}"
        }
    },    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "domain": {
                        "type": "string"
                    }
                }
            },
            "inspector": {
                "inputs": {
                    "domain": {
                        "type": "text",
                        "tooltip": "The domain name to add for email sending.",
                        "label": "Domain",
                        "index": 0
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "options": [
                {
                    "label": "Id",
                    "value": "id",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Domain",
                    "value": "domain",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Status",
                    "value": "status",
                    "schema": {
                        "type": "string"
                    }
                }
            ]
        }
    ],
    "icon": "data:image/svg+xml;base64,"
}
