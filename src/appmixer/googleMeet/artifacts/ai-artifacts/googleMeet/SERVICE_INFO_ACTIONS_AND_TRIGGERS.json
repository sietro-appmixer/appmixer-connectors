        {
            "title": "CreateSpace",
            "description": "Create a new Google Meet space (meeting). Requires OAuth 2.0; scope: https://www.googleapis.com/auth/meetings.space.created.",
            "type": "action",
            "documentation": "https://developers.google.com/workspace/meet/api/reference/rest/v2/spaces/create",
            "endpointUrl": "/v2/spaces",
            "apiBaseUrl": "https://meet.googleapis.com",
            "method": "POST",
            "sampleResponse": {
                "name": "spaces/abc123",
                "meetingCode": "abc-defg-hij",
                "meetingUri": "https://meet.google.com/abc-defg-hij",
                "spaceType": "MEETING"
            },
            "inputSchema": {
                "type": "object",
                "properties": {
                    "space": {
                        "type": "object",
                        "description": "Space resource to create. If omitted, a default meeting space is created.",
                        "properties": {
                            "spaceType": {
                                "type": "string",
                                "description": "Type of the space to create, for example MEETING."
                            },
                            "config": {
                                "type": "object",
                                "description": "Optional space configuration and settings (see API docs for supported fields).",
                                "additionalProperties": true
                            }
                        }
                    }
                }
            },
            "outputType": {
                "outputType": "object"
            }
        },
        {
