{
    "name": "appmixer.googleMeet.space.CreateSpace",
    "label": "Create Space",
    "author": "Appmixer <info@appmixer.com>",
    "description": "Create a new Google Meet space (meeting).",
    "version": "1.0.0",
    "private": false,
    "auth": {
        "service": "appmixer:googleMeet",
        "scope": [
            "https://www.googleapis.com/auth/meetings.space.created"
        ]
    },
    "quota": {
        "manager": "appmixer:googleMeet",
        "resources": "requests"
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "accessType": {
                        "type": "string"
                    },
                    "entryPointAccess": {
                        "type": "string"
                    },
                    "moderation": {
                        "type": "string"
                    },
                    "chatRestriction": {
                        "type": "string"
                    },
                    "reactionRestriction": {
                        "type": "string"
                    },
                    "presentRestriction": {
                        "type": "string"
                    },
                    "defaultJoinAsViewerType": {
                        "type": "string"
                    },
                    "attendanceReportGenerationType": {
                        "type": "string"
                    },
                    "autoRecordingGeneration": {
                        "type": "string"
                    },
                    "autoTranscriptionGeneration": {
                        "type": "string"
                    },
                    "autoSmartNotesGeneration": {
                        "type": "string"
                    }
                }
            },
            "inspector": {
                "inputs": {
                    "accessType": {
                        "group": "space",
                        "type": "select",
                        "label": "Access Type",
                        "tooltip": "Access type determines who can join without knocking",
                        "index": 0,
                        "options": [
                            {
                                "label": "Organization Default",
                                "value": "ACCESS_TYPE_UNSPECIFIED"
                            },
                            {
                                "label": "Open - Anyone with join info can join",
                                "value": "OPEN"
                            },
                            {
                                "label": "Trusted - Organization members and invitees can join",
                                "value": "TRUSTED"
                            },
                            {
                                "label": "Restricted - Only invitees can join",
                                "value": "RESTRICTED"
                            }
                        ]
                    },
                    "entryPointAccess": {
                        "group": "space",
                        "type": "select",
                        "label": "Entry Point Access",
                        "tooltip": "Controls which entry points can be used to join meetings",
                        "index": 1,
                        "options": [
                            {
                                "clearItem": true,
                                "label": "<User policy default>"
                            },
                            {
                                "label": "All entry points allowed",
                                "value": "ALL"
                            },
                            {
                                "label": "Creator app only",
                                "value": "CREATOR_APP_ONLY"
                            }
                        ]
                    },
                    "moderation": {
                        "group": "space",
                        "type": "select",
                        "label": "Moderation",
                        "tooltip": "Pre-configured moderation mode for the meeting",
                        "index": 2,
                        "options": [
                            {
                                "label": "Moderation On",
                                "value": "ON"
                            },
                            {
                                "label": "Moderation Off",
                                "value": "OFF"
                            }
                        ]
                    },
                    "chatRestriction": {
                        "group": "moderation",
                        "type": "select",
                        "label": "Chat Restriction",
                        "tooltip": "Defines who has permission to send chat messages in the meeting space. Only goes into effect when moderation is ON.",
                        "index": 3,
                        "options": [
                            {
                                "clearItem": true,
                                "label": "<User policy default>"
                            },
                            {
                                "label": "Meeting owner and co-host have the permission.",
                                "value": "HOSTS_ONLY"
                            },
                            {
                                "label": "All Participants have permissions",
                                "value": "NO_RESTRICTION"
                            }
                        ]
                    },
                    "reactionRestriction": {
                        "group": "moderation",
                        "type": "select",
                        "label": "Reaction Restriction",
                        "tooltip": "Defines who has permission to send reactions in the meeting space. Only goes into effect when moderation is ON.",
                        "index": 4,
                        "options": [
                            {
                                "clearItem": true,
                                "label": "<User policy default>"
                            },
                            {
                                "label": "Meeting owner and co-host have the permission",
                                "value": "HOSTS_ONLY"
                            },
                            {
                                "label": "All Participants have permissions",
                                "value": "NO_RESTRICTION"
                            }
                        ]
                    },
                    "presentRestriction": {
                        "group": "moderation",
                        "type": "select",
                        "label": "Present Restriction",
                        "tooltip": "Defines who has permission to share their screen in the meeting space. Only goes into effect when moderation is ON.",
                        "index": 5,
                        "options": [
                            {
                                "clearItem": true,
                                "label": "<User policy default>"
                            },
                            {
                                "label": "Meeting owner and co-host have the permission",
                                "value": "HOSTS_ONLY"
                            },
                            {
                                "label": "All Participants have permissions",
                                "value": "NO_RESTRICTION"
                            }
                        ]
                    },
                    "defaultJoinAsViewerType": {
                        "group": "moderation",
                        "type": "select",
                        "label": "Default Join As Viewer",
                        "tooltip": "Defines whether to restrict the default role assigned to users as viewer. Only goes into effect when moderation is ON.",
                        "index": 6,
                        "options": [
                            {
                                "clearItem": true,
                                "label": "<User policy default>"
                            },
                            {
                                "label": "Users will by default join as viewers",
                                "value": "ON"
                            },
                            {
                                "label": "Users will by default join as contributors",
                                "value": "OFF"
                            }
                        ]
                    },
                    "attendanceReportGenerationType": {
                        "group": "space",
                        "type": "select",
                        "label": "Attendance Report Generation",
                        "tooltip": "Whether attendance report is enabled for the meeting space",
                        "index": 7,
                        "options": [
                            {
                                "clearItem": true,
                                "label": "<User policy default>"
                            },
                            {
                                "label": "Generate Report",
                                "value": "GENERATE_REPORT"
                            },
                            {
                                "label": "Do Not Generate",
                                "value": "DO_NOT_GENERATE"
                            }
                        ]
                    },
                    "autoRecordingGeneration": {
                        "group": "artifactConfig",
                        "type": "select",
                        "label": "Auto Recording Generation",
                        "tooltip": "Whether meeting is automatically recorded when someone with record privilege joins",
                        "index": 8,
                        "options": [
                            {
                                "clearItem": true,
                                "label": "<User policy default>"
                            },
                            {
                                "label": "Auto Recording On",
                                "value": "ON"
                            },
                            {
                                "label": "Auto Recording Off",
                                "value": "OFF"
                            }
                        ]
                    },
                    "autoTranscriptionGeneration": {
                        "group": "artifactConfig",
                        "type": "select",
                        "label": "Auto Transcription Generation",
                        "tooltip": "Whether meeting content is automatically transcribed when someone with transcribe privilege joins",
                        "index": 9,
                        "options": [
                            {
                                "clearItem": true,
                                "label": "<User policy default>"
                            },
                            {
                                "label": "Auto Transcription On",
                                "value": "ON"
                            },
                            {
                                "label": "Auto Transcription Off",
                                "value": "OFF"
                            }
                        ]
                    },
                    "autoSmartNotesGeneration": {
                        "group": "artifactConfig",
                        "type": "select",
                        "label": "Auto Smart Notes Generation",
                        "tooltip": "Whether to automatically generate meeting summary and recap for organization invitees",
                        "index": 10,
                        "options": [
                            {
                                "clearItem": true,
                                "label": "<User policy default>"
                            },
                            {
                                "label": "Auto Smart Notes On",
                                "value": "ON"
                            },
                            {
                                "label": "Auto Smart Notes Off",
                                "value": "OFF"
                            }
                        ]
                    }
                },
                "groups": {
                    "space": {
                        "label": "Space",
                        "index": 0
                    },
                    "moderation": {
                        "label": "Moderation",
                        "index": 1
                    },
                    "artifactConfig": {
                        "label": "Artifact Config",
                        "index": 2
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "options": [
                {
                    "label": "Space ID",
                    "value": "name",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Meeting Uri",
                    "value": "meetingUri",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Meeting Code",
                    "value": "meetingCode",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Config",
                    "value": "config",
                    "schema": {
                        "type": "object",
                        "properties": {
                            "accessType": {
                                "type": "string",
                                "title": "Config.Access Type"
                            },
                            "entryPointAccess": {
                                "type": "string",
                                "title": "Config.Entry Point Access"
                            },
                            "moderation": {
                                "type": "string",
                                "title": "Config.Moderation"
                            },
                            "moderationRestrictions": {
                                "type": "object",
                                "properties": {
                                    "chatRestriction": {
                                        "type": "string",
                                        "title": "Config.Moderation Restrictions.Chat Restriction"
                                    },
                                    "reactionRestriction": {
                                        "type": "string",
                                        "title": "Config.Moderation Restrictions.Reaction Restriction"
                                    },
                                    "presentRestriction": {
                                        "type": "string",
                                        "title": "Config.Moderation Restrictions.Present Restriction"
                                    },
                                    "defaultJoinAsViewerType": {
                                        "type": "string",
                                        "title": "Config.Moderation Restrictions.Default Join As Viewer Type"
                                    }
                                },
                                "title": "Config.Moderation Restrictions"
                            },
                            "attendanceReportGenerationType": {
                                "type": "string",
                                "title": "Config.Attendance Report Generation Type"
                            },
                            "artifactConfig": {
                                "type": "object",
                                "properties": {
                                    "recordingConfig": {
                                        "type": "object",
                                        "properties": {
                                            "autoRecordingGeneration": {
                                                "type": "string",
                                                "title": "Config.Artifact Config.Recording Config.Auto Recording Generation"
                                            }
                                        },
                                        "title": "Config.Artifact Config.Recording Config"
                                    },
                                    "transcriptionConfig": {
                                        "type": "object",
                                        "properties": {
                                            "autoTranscriptionGeneration": {
                                                "type": "string",
                                                "title": "Config.Artifact Config.Transcription Config.Auto Transcription Generation"
                                            }
                                        },
                                        "title": "Config.Artifact Config.Transcription Config"
                                    },
                                    "smartNotesConfig": {
                                        "type": "object",
                                        "properties": {
                                            "autoSmartNotesGeneration": {
                                                "type": "string",
                                                "title": "Config.Artifact Config.Smart Notes Config.Auto Smart Notes Generation"
                                            }
                                        },
                                        "title": "Config.Artifact Config.Smart Notes Config"
                                    }
                                },
                                "title": "Config.Artifact Config"
                            }
                        }
                    }
                },
                {
                    "label": "Active Conference",
                    "value": "activeConference",
                    "schema": {
                        "type": "object",
                        "properties": {
                            "conferenceRecord": {
                                "type": "string",
                                "title": "Active Conference.Conference Record"
                            }
                        }
                    }
                }
            ]
        }
    ],
    "icon": "data:image/svg+xml;base64,"
}
