{
    "name": "appmixer.googleMeet.space.GetSpace",
    "label": "Get Space",
    "author": "Appmixer <info@appmixer.com>",
    "description": "Retrieve details of a Meet space.",
    "version": "1.0.0",
    "private": false,
    "auth": {
        "service": "appmixer:googleMeet",
        "scope": [
            "https://www.googleapis.com/auth/meetings.space.readonly"
        ]
    },
    "quota": {
        "manager": "appmixer:googleMeet",
        "resources": "requests"
    },
    "inPorts": [
        {
            "name": "in",
            "schema": {
                "type": "object",
                "properties": {
                    "name": {
                        "type": "string"
                    }
                },
                "required": [
                    "name"
                ]
            },
            "inspector": {
                "inputs": {
                    "name": {
                        "type": "text",
                        "tooltip": "Enter either Space ID or Meeting Code. Space ID example: jQCFfuBOdN5z. Meeting Code example: abc-mnop-xyz",
                        "label": "Space ID or Meeting Code",
                        "index": 0
                    }
                }
            }
        }
    ],
    "outPorts": [
        {
            "name": "out",
            "options": [
                {
                    "label": "Space ID",
                    "value": "name",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Meeting Uri",
                    "value": "meetingUri",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Meeting Code",
                    "value": "meetingCode",
                    "schema": {
                        "type": "string"
                    }
                },
                {
                    "label": "Config",
                    "value": "config",
                    "schema": {
                        "type": "object",
                        "properties": {
                            "accessType": {
                                "type": "string",
                                "title": "Config.Access Type"
                            },
                            "entryPointAccess": {
                                "type": "string",
                                "title": "Config.Entry Point Access"
                            },
                            "moderation": {
                                "type": "string",
                                "title": "Config.Moderation"
                            },
                            "moderationRestrictions": {
                                "type": "object",
                                "properties": {
                                    "chatRestriction": {
                                        "type": "string",
                                        "title": "Config.Moderation Restrictions.Chat Restriction"
                                    },
                                    "reactionRestriction": {
                                        "type": "string",
                                        "title": "Config.Moderation Restrictions.Reaction Restriction"
                                    },
                                    "presentRestriction": {
                                        "type": "string",
                                        "title": "Config.Moderation Restrictions.Present Restriction"
                                    },
                                    "defaultJoinAsViewerType": {
                                        "type": "string",
                                        "title": "Config.Moderation Restrictions.Default Join As Viewer Type"
                                    }
                                },
                                "title": "Config.Moderation Restrictions"
                            },
                            "attendanceReportGenerationType": {
                                "type": "string",
                                "title": "Config.Attendance Report Generation Type"
                            },
                            "artifactConfig": {
                                "type": "object",
                                "properties": {
                                    "recordingConfig": {
                                        "type": "object",
                                        "properties": {
                                            "autoRecordingGeneration": {
                                                "type": "string",
                                                "title": "Config.Artifact Config.Recording Config.Auto Recording Generation"
                                            }
                                        },
                                        "title": "Config.Artifact Config.Recording Config"
                                    },
                                    "transcriptionConfig": {
                                        "type": "object",
                                        "properties": {
                                            "autoTranscriptionGeneration": {
                                                "type": "string",
                                                "title": "Config.Artifact Config.Transcription Config.Auto Transcription Generation"
                                            }
                                        },
                                        "title": "Config.Artifact Config.Transcription Config"
                                    },
                                    "smartNotesConfig": {
                                        "type": "object",
                                        "properties": {
                                            "autoSmartNotesGeneration": {
                                                "type": "string",
                                                "title": "Config.Artifact Config.Smart Notes Config.Auto Smart Notes Generation"
                                            }
                                        },
                                        "title": "Config.Artifact Config.Smart Notes Config"
                                    }
                                },
                                "title": "Config.Artifact Config"
                            }
                        }
                    }
                },
                {
                    "label": "Active Conference",
                    "value": "activeConference",
                    "schema": {
                        "type": "object",
                        "properties": {
                            "conferenceRecord": {
                                "type": "string",
                                "title": "Active Conference.Conference Record"
                            }
                        }
                    }
                }
            ]
        }
    ],
    "icon": "data:image/svg+xml;base64,"
}
